<div class="row-fluid">
    <div class="span12">
        <select data-placeholder=" " name="{{name}}" {{#if def.multi}} multiple {{/if}}>
            {{#eachOptions def.options}}
            <option value="{{{this.key}}}" {{#has this.key ../def.value}}selected{{/has}}>{{this.value}}</option>
            {{/eachOptions}}
        </select>
        <p class="help-block"></p>
    </div>
</div>