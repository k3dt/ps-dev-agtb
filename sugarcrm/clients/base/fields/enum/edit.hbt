        <select data-placeholder=" " name="{{name}}" {{#if def.multi}} multiple {{/if}}>
            <option value="" selected></option>
            {{#eachOptions def.options}}
            <option value="{{{this.key}}}" {{#has this.key ../value}}selected{{/has}}>{{this.value}}</option>
            {{/eachOptions}}
        </select>
        <p class="help-block"></p>