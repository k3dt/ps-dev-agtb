<div class="row-fluid">
    <div class="span2"><strong>{{label}}</strong></div>
    <div class="span4">
        <select name="{{name}}" {{#if multi}} multiple {{/if}}>{{#eachOptions options}}<option value="{{{this.key}}}" {{has this.key ../value "selected"}}>{{this.value}}</option>{{/eachOptions}}</select>
        <p class="help-block"></p>
    </div>
</div>