        <select data-placeholder="{{str "LBL_MODULE_NAME" def.module}}" name="{{name}}" class="relateEdit" {{#if multi}} multiple {{/if}}>
            <option value="{{this.value}}" selected>{{this.value}}</option>
            {{#eachOptions selectOptions}}
                <option value="{{getFieldValue this ../this.def.rname ""}}" id="{{getFieldValue this "id" ""}}">{{getFieldValue this ../this.def.rname ""}}</option>
            {{/eachOptions}}
        </select>
        <p class="help-block"></p>