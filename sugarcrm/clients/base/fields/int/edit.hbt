<div class="row-fluid">
    <div class="span2"><strong>{{label}}</strong></div>
    <div class="span4">
        {{#if auto_increment}}
            {{value}}
        {{else}}
            <input type="text" class="input-xlarge" value="{{value}}">
        {{/if}}
        <p class="help-block"></p>
    </div>
</div>