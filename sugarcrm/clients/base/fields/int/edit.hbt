<div class="control-group">
<div class="row-fluid">
    <div class="span2"><strong>{{getLabel label model.module}}</strong></div>
    <div class="span4">
        {{#if fieldDef.auto_increment}}
             {{eq value "NaN" "--" value}}
        {{else}}
            <input type="text" class="input-xlarge" value="{{value}}">
        {{/if}}
        <p class="help-block"></p>
    </div>
</div>
    </div>