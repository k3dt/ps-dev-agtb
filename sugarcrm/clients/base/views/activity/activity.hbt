<div class="card2">

                <div class="block filtered tabbable tabs-left" id="moduleActivity">
                    <h4>Activity</h4>

                    {{#each meta.panels}}
                    <ul class="nav nav-tabs results">
                        {{#if ../collection.models}}
                        {{#each ../../collection.models}}
                        <li class="activity" data-id="{{this.id}}">
                            <a data-toggle="tab"><span class="label label-note">Note</span></a>
                            <p>
                                <a>{{attributes.name}}</a>
                                <br>
                                <date class="date">
                                    {{timeago attributes.date_entered}}
                                </date>
                            </p>
                            <div class="actions">
                               <a><i class="icon-eye-open" rel="tooltip" data-title="Preview"></i></a>
                            </div>
                        </li>
                        {{/each}}
                        {{else}}
                            This record has no notes at this time. Please add a note by clicking on the add note link.
                        {{/if}}
                    </ul>
                    {{/each}}
                    <div class="actions">
                        <a class="btn addNote" rel="tooltip" title="Add a note or attachment"><i class="icon-plus"></i> Add Note</a>
                    </div>
                </div>
                {{#if collection.models}}
                {{#notEq collection.next_offset "-1"}}
                <div class="block-footer">
                    {{#each meta.buttons}}
                        {{field ../this ../model}}
                    {{/each}}
                </div>
                {{/notEq}}
                {{/if}}
            </div>
        </div>
    </div>
</div>
