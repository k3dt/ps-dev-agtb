{{#each meta.panels}}
<div class="tab-pane active">
    <div class="block" id="">
        <a data-toggle="tab" class="actions closeSubdetail"><i class="icon-remove"></i></a>
        <h4><span class="label label-note">Note</span> {{fieldWithName ../context ../this ../model "name" "default"}}</h4>
        <br>

        {{#each fields}}
        {{field ../../context ../../this ../../model}}
        {{/each}}
    </div>
</div>
{{/each}}