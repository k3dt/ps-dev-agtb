						<td>
							<div class='dnb-checkbox-nav'>
								{{#if product.dataIndicatorMap.billing_address_postalcode}}
									{{#eq product.dataIndicatorMap.billing_address_postalcode 'upd'}}<input type='checkbox' name='billing_address_postalcode' id='billing_address_postalcode' class='dnb_checkbox' checked/>{{/eq}}
