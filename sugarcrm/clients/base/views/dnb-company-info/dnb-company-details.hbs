					<tr>
						<td align="center">
							<div class='dnb-checkbox-nav'>
								{{#if product.dataIndicatorMap.sic_code}}
									{{#eq product.dataIndicatorMap.sic_code 'upd'}}<input type='checkbox' name='sic_code' id='sic_code' class='dnb_checkbox' checked/>{{/eq}}
									{{#eq product.dataIndicatorMap.sic_code 'dup'}}<input type='checkbox' name='sic_code' id='sic_code' class='dnb_checkbox'/>{{/eq}}

								{{else}}
									<input type="checkbox" name="sic_code" id="sic_code" class="dnb_checkbox" checked />
								{{/if}}
							</div>
							<div class="dnb-company-label">
								{{str "LBL_DNB_SIC"}}
							</div>
							<div class="txt importData">
								{{product.primarySIC.IndustryCode.$}}
								{{#if product.dataIndicatorMap.sic_code}}
	                            	{{#eq product.dataIndicatorMap.sic_code 'dup'}}
	                            		<span class="label label-warning pull-right" data-placement="right">{{str "LBL_DNB_DUPLICATE"}}</span>
	                            	{{/eq}}
	                            {{else}}
	                            	<span class="label label-important pull-right" data-placement="right">{{str "LBL_DNB_UPLOAD"}}</span>
	                        	{{/if}}
							</div>
						</td>
					</tr>
				{{/if}}
