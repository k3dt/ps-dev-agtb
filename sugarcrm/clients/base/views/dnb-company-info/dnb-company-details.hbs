	</div>
	{{else}}
		<aside>
			<em>{{errmsg}}</em>
		</aside>
	{{/if}}
	{{#if isNotLinked}}
		<div class="dnb-show-more">
		    <a class="backToList">{{str "LBL_DNB_BACK_TO_SRCH"}} <i class="icon-chevron-down"></i></a>
		</div>
	{{/if}}
</div>