                        {{#each data.records}}
                            <li>
                                <span>{{_module}},</span><a href="#{{_module}}/{{id}}"><strong>{{name}}</strong></a>
                            </li>
                        {{/each}}
                        {{#notEq data.next_offset "-1"}}
                            <li>
                                <a href="#search/{{term}}"><strong>{{str "LNK_SEARCH_FTS_VIEW_ALL"}}</a>
                            </li>
                        {{/notEq}}
