{{#if keywords.length}}
  <li class="li-group">
      {{str 'LBL_SWEETSPOT_KEYWORDS_SECTION'}}
  </li>
{{/if}}
{{#each keywords}}
    {{partial 'result' this component=../this}}
{{/each}}

{{#if actions.length}}
  <li class="li-group">
      {{str 'LBL_SWEETSPOT_ACTIONS_SECTION'}}
  </li>
{{/if}}
{{#each actions}}
    {{partial 'result' this component=../this}}
{{/each}}

{{#if records.length}}
  <li class="li-group">
      {{str 'LBL_SWEETSPOT_RECORDS_SECTION'}}
  </li>
{{/if}}
{{#each records}}
    {{partial 'result' this component=../this}}
{{/each}}
