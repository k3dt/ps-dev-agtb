<div class="row-fluid">
    <div class="block filtered tabbable">
        <ul class="nav search-results">
            {{#each collection.models}}
                <li class="search-result" data-id="{{attributes.id}}">
                    {{partial 'row' ../this ../this model=this module=module}}
                    {{#each ../meta.rowactions.actions}}
                        {{field ../../this model=../this}}
                    {{/each}}
                </li>
            {{/each}}
        </ul>
    </div>
</div>
