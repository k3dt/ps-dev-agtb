<div class="container-fluid">
<form name='{{name}}'>
    <div class="card2 span7">
        {{#each meta.panels}}
        {{#each fields}}
        {{field ../../this}}
        {{/each}}
        {{/each}}
        <div class="form-actions">
            {{#each meta.buttons}}
            {{field ../this}}
            {{/each}}
        </div>
    </div>
</form>
</div>
