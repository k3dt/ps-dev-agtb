<div class="block thumbnail widget">
    <div class="news-widget">
        <h4 data-tour="tour-{{module}}-news">News</h4>
        {{#each responseData.results}}
        <div class="news-article">
            <h5><a href="{{this.unescapedUrl}}">{{{this.titleNoFormatting}}}</a></h5> ({{{this.publisher}}})
            <p>{{{this.content}}}</p>
        </div>
        {{/each}}
    </div>
</div>
