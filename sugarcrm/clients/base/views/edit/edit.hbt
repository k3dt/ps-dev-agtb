<form name='{{name}}'>
    <div class="card2">
        {{#each meta.panels}}
        {{#each fields}}
        {{field ../../context ../../this ../../model}}
        {{/each}}
        {{/each}}
        <div class="form-actions">
            {{#each meta.buttons}}
            {{field ../context ../this ../model}}
            {{/each}}
        </div>
    </div>
</form>