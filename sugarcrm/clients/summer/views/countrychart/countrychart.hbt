<div class="block">
  <h4>Sales</h4>
    {{#if urls}}
      <select class="chartSelector">
          {{#each urls}}
              <option value="{{this.url}}">{{this.title}}</option>
          {{/each}}
      </select>
    {{/if}}
  <div id="{{guid}}"></div>
</div>
<style>
    .block path {
        stroke: #fff;
    }
</style>
<!-- TODO: Remove this once d3's native. -->
<script src="../styleguide/styleguide/js/nvd3/lib/d3.v2.js"></script>
