<div class="block thumbnail padded">
  <h4>Sales by Country</h4>
    {{#if urls}}
      <select class="chartSelector">
          {{#each urls}}
              <option value="{{this.url}}">{{this.title}}</option>
          {{/each}}
      </select>
    {{/if}}
  <div id="{{guid}}"></div>
</div>
<style>
    .block path {
        stroke: #fff;
    }
</style>
