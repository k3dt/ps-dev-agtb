{{#each meta.panels}}
<div class="modal-body">
    <h2>{{str label "Forecasts"}}</h2>
    {{#each fields}}
        <label for="{{name}}">{{str label "Forecasts"}}</label>
        {{field ../../this ../../model}}
    {{/each}}
</div>
<div class="modal-footer">
    {{#each buttons}}
        {{field ../../this ../../model}}
    {{/each}}
</div>
{{/each}}