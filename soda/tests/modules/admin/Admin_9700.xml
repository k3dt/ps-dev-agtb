<soda>
	<puts text="Test: Admin_9700.xml starting." />

	<!--Nav to Administration module-->
	<link href="index.php?module=Administration&amp;action=index" />
	<!--Nav to Connectors Settings-->
	<link text="Connectors" />
	<div class="moduleTitle">
		<browser assert="Connector Settings" />
	</div>
	<!--Nav to Connector Properties Configuration-->
	<image name="connectorConfig" click="true" />
	<wait timeout="15" />
	<!--Click on the "Test Connector" button-->
	<link text="/Hoovers/" />
	<wait timeout="15" />
	<button id="ext_soap_hoovers_test_button" />
	<wait timeout="45" />
	<span id="ext_soap_hoovers_result" assert="/Test Successful/" />
	<button value="Cancel" />

	<puts text="Test: Admin_9700.xml finished." />
</soda>