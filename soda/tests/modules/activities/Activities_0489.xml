<soda>
         <puts text='------------Activities_0489:Create Note with html file as attachment_Verify that the .html type attachment---'/>
    
         
         <!--create note record-->
         <link id='moduleTab_Activities'/>
	 <browser assert="Task List"/>
         <link text='Create Note or Add Attachment'/>

         <csv file="{@global.scriptsdir}/modules/lib/Note_Create_Lib.csv" var="note_record_0489">
             <textfield id='name' set='{@note_record_0489.name} {@user.name} {@stamp}'/>
             <textfield id='description' set='{@note_record_0489.description}'/>
             <filefield name='uploadfile' set='{@global.scriptsdir}/export/note.html'/>
            
             <!--teams select-->
             <table id='EditView_team_name_table'>
                 <button value='Select'/>
             </table>
             <browser assert='Team Search'/>
             <textfield name='name' set='{@note_record_0489.team_name}'/> 
             <button value='Search'/>
             <link text='{@note_record_0489.team_name}'/>
         </csv>
         <button value="Save" />
         <var var="pre_note_name_0489" set="{@note_record_0489.name} {@user.name} {@stamp}" />
         <puts text='------------Activities_0489 is Finished!------------'/>
</soda>