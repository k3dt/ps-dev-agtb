<soda>
	<!-- Duplicate Note_Verify that duplicating a note can be canceled -->
	<puts text="Test: Activities_0502.xml starting" />


	<!--create call record-->
	<script file="{@global.scriptsdir}/modules/lib/Note_Create_Lib.xml" />

	<link id="moduleTab_Activities" />
	<browser assert="Task List" />
	<link text="View Notes" />

	<button id="search_form_clear" />
	<textfield id="name_basic" set="{@pre_note_name}" />
	<button id="search_form_submit" />
	<table class="list view">
		<link text="{@pre_note_name}" />
	</table>

	<button id="duplicate_button" />
	<csv file="{@global.scriptsdir}/csvs/activities/Activities_0502.csv" var="activity_record_0502">
		<textfield id="name" set="{@activity_record_0502.name} {@user.name} {@stamp}" />
		<textfield id="description" set="{@activity_record_0502.description}" />

		<!--teams select-->
		<table id="EditView_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@activity_record_0502.team_name}" />
		<button value="Search" />
		<link text="{@activity_record_0502.team_name}" />

		<button value="Cancel" alert="true" />
	</csv>

	<browser assert="Notes" />

	<script file="{@global.scriptsdir}/modules/lib/Note_Delete_Lib.xml" />
	<puts text="Test: Activities_0502.xml finished" />
</soda>