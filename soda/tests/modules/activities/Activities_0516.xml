<soda>
	<!-- Create Note_Verify that note is created for call/meeting from -->
	<puts text="Test: Activities_0516.xml starting." />


	<!--create call record-->
	<script file="{@global.scriptsdir}/modules/lib/Meeting_Create_Lib.xml" />


	<link id="moduleTab_Notes" />
	<browser assert="Notes" />
	<link text="Create Note or Attachment" />

	<csv file="{@global.scriptsdir}/modules/lib/Note_Create_Lib.csv" var="note_record_0790">
		<textfield id="name" set="{@note_record_0790.name}" />
		<textfield id="description" set="{@note_record_0790.description}" />

		<!--teams select-->
		<table id="EditView_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@note_record_0790.team_name}" />
		<button value="Search" />
		<link text="{@note_record_0790.team_name}" />

		<select id="parent_type" set="Meeting" />
		<wait timeout="3" />
		<button id="btn_parent_name" />
		<browser assert="Meeting Search" />
		<textfield id="name_advanced" set="{@pre_meeting_name}" />
		<button value="Search" />
		<wait timeout="3" />
		<link text="{@pre_meeting_name}" />

		<button value="Save" />
		<wait timeout="5" />
		<browser assert="{@note_record_0790.name}" />
	</csv>
	<link id="moduleTab_Meetings" />
	<link text="View Meetings" />

	<button id="search_form_clear" />
	<textfield id="name_basic" set="{@pre_meeting_name}" />
	<button id="search_form_submit" />
	<table class="list view">
		<link text="{@pre_meeting_name}" />
	</table>

	<div id="list_subpanel_history">
		<link text="{@note_record_0790.name}" click="false" />
	</div>


	<script file="{@global.scriptsdir}/modules/lib/Meeting_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Note_Delete_Lib.xml" />
	<puts text="Test: Activities_0516.xml finished." />
</soda>