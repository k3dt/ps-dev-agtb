<soda>
	<puts text="Test: Activities_10065 starting." />
	
	<link id="moduleTab_Notes" />
	<link text="Create Note or Attachment" />

	<csv file="{@global.scriptsdir}/csvs/activities/Activities_10062.csv" var="note_record">
		<textfield id="name" set="{@note_record.name}" />
		<button id="btn_assigned_user_name" />
		<link text="{@note_record.assigned_user_name}" />
	</csv>

	<button value="Save" />

	<table id="detailpanel_2">
		<row index="1">
			<cell index="2" assert="{@note_record.assigned_user_name}" />
		</row>
	</table>

	<button id="edit_button" />

	<button id="btn_clr_assigned_user_name" />

	<button value="Save" />

	<table id="detailpanel_2">
		<row index="1">
			<cell index="2" assertnot="{@note_record.assigned_user_name}" />
		</row>
	</table>

	<script file="{@global.scriptsdir}/modules/lib/Note_Delete_Lib.xml" />

	<puts text="Test: Activities_10065 finished." />

</soda>