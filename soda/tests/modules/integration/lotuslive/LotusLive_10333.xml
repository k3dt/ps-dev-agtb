<soda>
	<puts text="Test: LotusLive_10333.xml starting." />
	<link id="moduleTab_Quotes"/>
	<button   id="search_form_clear"/>
        <form id="search_form">
            <textfield id="name_basic" set="{@pre_Quote_name}"/>
        </form>
    <button id="search_form_submit"/>
    <table class="list view">
        <link text="{@pre_Quote_name}"/>
    </table>
    <browser assert="{@pre_Quote_name}"/>
	
	<button name="documents_quotes_create_button" />
	<div id="form_SubpanelQuickCreate_Documents_tabs">
		<timestamp />
		<select id="doc_type" set="LotusLive" />
		<textfield name="document_name" set="LotusLive_Sub_Pptx" /> 
		<filefield id="filename_file" set="{@global.scriptsdir}/csvs/3.pptx" />	
	</div>
	<button id="Documents_subpanel_save_button" />
	<wait timeout='10'/>
	<link text="3.pptx" click="false" exist="true"/>

	<button name="documents_quotes_create_button" />
	<button id="Documents_subpanel_full_form_button" />
	<form id="EditView">
		<timestamp />
		<select id="doc_type" set="LotusLive" />
		<textfield name="document_name" set="LotusLive_Sub_Pdf" /> 
		<wait timeout='2' />
		<filefield id="filename_file" set="{@global.scriptsdir}/csvs/4.pdf" />
		<button value="Save" />
	</form>
	<link text="4.pdf" click="false" exist="true"/>

	<link class='listViewTdToolsS1' index='2' alert='true' />
	<wait timeout='3'/>
	<link class='listViewTdToolsS1' index='2' alert='true' />
	<wait timeout='3'/>
	<link text="3.pptx" click="false" exist="false"/>
	<link text="4.pdf" click="false" exist="false"/>

	<link id="moduleTab_Documents" />
	<link text="View Documents" />

	<button id="search_form_clear" />
	<textfield id="document_name_basic" set="LotusLive_Sub" />
	<button id="search_form_submit" />
	<span class="pageNumbers" assert="(1 - 2 of 2)" />

	<checkbox name="massall" click="true" />
	<link id="actions_link" />
	<link text="Delete" alert="true" />
	<span class="pageNumbers" assert="(0 - 0 of 0)" />

	
	<puts text="Test: LotusLive_10333.xml finished." />
</soda>
