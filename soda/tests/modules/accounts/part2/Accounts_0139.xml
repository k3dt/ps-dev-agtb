<soda>
	<!--
		Search Account: Verify that error message "Missing required field: Name" is
		displayed when searching with variant search condition.
	-->
	<puts text="Test: Accounts_0139.xml starting." />
	<!--
		Since we don"t have sample data installed in our testing Sugar instance, this
		test case always passes the test.
	-->

	<!--Search the record-->
	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>
	<link text="Basic Search" required="false" />
	<csv file="{@global.scriptsdir}/csvs/accounts/Accounts_0139.csv" var="account_0139">
		<form id="search_form">
			<button id="search_form_clear" />
			<textfield id="name_basic" set="{@account_0139.name_basic}" />
			<button id="search_form_submit" />
		</form>

		<table class="list view">
			<span class="pageNumbers" assert="(0 - 0 of 0)" />
		</table>
	</csv>
	<puts text="Test: Accounts_0139.xml finished." />

</soda>
