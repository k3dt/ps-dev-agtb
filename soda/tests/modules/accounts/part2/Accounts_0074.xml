<soda>
	<puts text="Test: Accounts_0074.xml starting." />

	<!--Create a account record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Case_Create_Lib.xml" />

	<!--Search the record-->
	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>
	<textfield id="name_basic" set="{@pre_account_name}" />
	<button id="search_form_submit" />
	<table class="list view">
		<link text="{@pre_account_name}" />
	</table>

	<!--Go case subpanel & click "Select"-->
	<browser assert="{@pre_account_name}" />
	<button id="account_cases_select_button" />
	<table class="list view">
		<link text="{@pre_case_name}" />
	</table>

	<!--back to case subpanel-->
	<div id="list_subpanel_cases">
		<table class="list view">
			<link text="{@pre_case_name}" click="false" />
		</table>
	</div>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Case_Delete_Lib.xml" />

	<puts text="Test: Accounts_0074.xml finished." />

</soda>
            
