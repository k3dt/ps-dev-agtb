<soda>
	<puts text="Test: Accounts_0003.xml starting." />

	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>

	<csv file="{@global.scriptsdir}/csvs/accounts/Accounts_0001.csv" var="account_record_0003">

		<link text="Create Account" />
		<div class="moduleTitle">
			<link href="index.php?module=Accounts&amp;action=index" click="false" />
			<browser assert="Create" />
		</div>

		<!--user select-->
		<button id="btn_assigned_user_name" />
		<browser assert="User Search" />
		<textfield name="last_name_advanced" set="{@account_record_0003.assigned_user_name}" />
		<button value="Search" />
		<link text="{@account_record_0003.assigned_user_name}" />

		<!--teams select-->
		<table id="EditView_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@account_record_0003.team_name}" />
		<button value="Search" />
		<link text="{@account_record_0003.team_name}" />

		<form id="EditView">
			<textfield id="name" set="{@account_record_0003.name} {@user.name} {@stamp}" />

			<textfield id="website" set="{@account_record_0003.website}" />

			<textfield id="ticker_symbol" set="{@account_record_0003.ticker_symbol}" />

			<textfield id="ownership" set="{@account_record_0003.ownership}" />

			<select id="industry" set="{@account_record_0003.industry}" />

			<select id="account_type" set="{@account_record_0003.account_type}" />

			<textfield id="phone_office" set="{@account_record_0003.phone_office}" />

			<textfield id="phone_fax" set="{@account_record_0003.phone_fax}" />

			<textfield id="employees" set="{@account_record_0003.employees}" />

			<textfield id="rating" set="{@account_record_0003.rating}" />

			<textfield id="sic_code" set="{@account_record_0003.sic_code}" />

			<textfield id="annual_revenue" set="{@account_record_0003.annual_revenue}" />

			<textfield id="billing_address_street" set="{@account_record_0003.billing_address_street}" />

			<textfield id="billing_address_city" set="{@account_record_0003.billing_address_city}" />

			<textfield id="billing_address_state" set="{@account_record_0003.billing_address_state}" />

			<textfield id="billing_address_postalcode" set="{@account_record_0003.billing_address_postalcode}" />

			<textfield id="billing_address_country" set="{@account_record_0003.billing_address_country}" />

			<textfield id="shipping_address_street" set="{@account_record_0003.shipping_address_street}" />

			<textfield id="shipping_address_city" set="{@account_record_0003.shipping_address_city}" />

			<textfield id="shipping_address_state" set="{@account_record_0003.shipping_address_state}" />

			<textfield id="shipping_address_postalcode" set="{@account_record_0003.shipping_address_postalcode}" />

			<textfield id="shipping_address_country" set="{@account_record_0003.shipping_address_country}" />

			<textfield id="Accounts0emailAddress0" set="{@account_record_0003.emailAddress0}" />

			<textfield id="description" set="{@account_record_0003.description}" />

			<button value="Cancel" alert="true" />
		</form>

		<div class="moduleTitle">
			<link href="index.php?module=Accounts&amp;action=index" click="false" />
		</div>
		<textfield id="name_basic" set="{@account_record_0003.name} {@user.name} {@stamp}" />
		<button id="search_form_submit" />
		<table class="list view">
			<span class="pageNumbers" assert="(0 - 0 of 0)" />
		</table>
	</csv>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />

	<puts text="Test: Accounts_0003.xml finished." />
</soda>
