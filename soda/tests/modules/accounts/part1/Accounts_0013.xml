<soda>
	<puts text="Test: Accounts_0013.xml starting." />

	<!--Create a account record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib_FULL.xml" />

	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>
	<wait timeout="3" />
	<link text="/Advanced Search/" />
	<csv file="{@global.scriptsdir}/csvs/accounts/Accounts_0001.csv" var="account_record_0013">
		<textfield id="name_advanced" set="{@pre_account_name}" />

		<textfield id="website_advanced" set="{@account_record_0013.website}" />

		<select name="industry_advanced[]" set="{@account_record_0013.industry}" />

		<select name="account_type_advanced[]" set="{@account_record_0013.account_type}" />

		<textfield id="phone_advanced" set="{@account_record_0013.phone_office}" />

		<textfield id="email_advanced" set="{@pre_account_email}" />
	</csv>
	<button id="search_form_submit" />
	<wait timeout="5" />
	<table class="list view">
		<link text="{@pre_account_name}" />
	</table>
	<wait timeout="3" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
		<browser assert="{@pre_account_name}" />
	</div>

	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>
	<wait timeout="3" />
	<link text="Basic Search" />
	<wait timeout="3" />
	<button id="search_form_clear" />
	<button id="search_form_submit" />
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />

	<puts text="Test: Accounts_0013.xml finished." />

</soda>
