<soda>
	<puts text="Test: Accounts_0010.xml starting." />

	<!--Create a account record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.xml" />

	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>
	<link text="/Basic Search/" required="false" timeout="2" />
	<csv file="{@global.scriptsdir}/csvs/accounts/Accounts_0001.csv" var="account_record_0010">
		<button id="search_form_clear" />
		<textfield id="name_basic" set="{@account_record_0010.name}" />
