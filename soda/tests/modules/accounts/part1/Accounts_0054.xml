<soda>
	<puts text="Test: Accounts_0054.xmlstarting." />

	<!--Create a account record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.xml" />

	<!--Search the record-->
	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>
	<textfield id="name_basic" set="{@pre_account_name}" />
	<button id="search_form_submit" />
	<table class="list view">
		<link text="{@pre_account_name}" />
	</table>

	<!--Go its detail view-->
	<browser assert="{@pre_account_name}" />

	<!--Create History Subpanel-->
	<button name="History_create_button" />
	<wait timeout="3" />
	<csv file="{@global.scriptsdir}/csvs/accounts/Accounts_0036.csv" var="history_record_0054">
		<form id="form_SubpanelQuickCreate_Notes">
			<textfield id="name" set="{@history_record_0054.name} {@user.name} {@stamp}" />
			<textfield id="description" set="{@history_record_0054.description}" />
			<filefield id="filename_file" set="{@history_record_0054.uploadfile}" />
		</form>

		<!--teams select-->
		<table id="form_SubpanelQuickCreate_Notes_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@history_record_0054.team_name}" />
		<button value="Search" />
		<link text="{@history_record_0054.team_name}" />
		<button id="Notes_subpanel_save_button" />
		<wait />
		<!--Verify the history record has been created-->
		<link id="moduleTab_Accounts" />
		<div class="moduleTitle">
			<link href="index.php?module=Accounts&amp;action=index" click="false" />
		</div>
		<textfield id="name_basic" set="{@pre_account_name}" />
		<button id="search_form_submit" />
		<table class="list view">
			<link text="{@pre_account_name}" />
		</table>
		<wait timeout="3" />
		<div id="list_subpanel_history">
			<table class="list view">
				<span class="pageNumbers" assert="(1 - 1 of 1)" />
			</table>
		</div>
	</csv>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<puts text="Test: Accounts_0054.xml finished." />
</soda>
