<soda>
	<puts text="Test: Accounts_0012.xml starting." />

	<!--Create a account record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.xml" />

	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>
	<csv file="{@global.scriptsdir}/csvs/accounts/Accounts_0001.csv" var="account_record_0012" />
	<button id="search_form_clear" />
	<wait timeout="3" />
	<textfield id="name_basic" set="{@account_record_0012.name}" />
	<button id="search_form_submit" />
	<wait timeout="5" />
	<browser assert="{@account_record_0012.name}" />

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<puts text="Test: Accounts_0012.xml finished." />
</soda>
