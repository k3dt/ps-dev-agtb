<soda>
	<!--
		Click “Cancel” button after do pagination and “Save and Continue” action on
		edit view
	-->
	<puts text="Test: Accounts_10264.xml starting." />

	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib_MIN.xml" />
	<var var="account1_name" set="{@pre_account_name}" />
	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib_MIN.xml" />
	<var var="account2_name" set="{@pre_account_name}" />
	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib_MIN.xml" />
	<var var="account3_name" set="{@pre_account_name}" />


	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>

	<!-- Click on the Pencil / Edit icon... -->
	<table class="list view">
		<row class="oddListRowS1">
			<cell index="3">
				<link index="1" click="true" jscriptevent="onclick" />
			</cell>
		</row>
	</table>
	<wait timeout="3" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
		<browser assert="{@account3_name}" />
	</div>
	
	<!-- Click on the EditView Pagination - Next... -->
	<span class="pagination">
		<button index="2" />
	</span>
	<browser assert="{@account2_name}" />
	<wait timeout="3" />

	<!-- Click on the EditView Pagination - Next... -->
	<span class="pagination">
		<button index="2" />
	</span>
	<browser assert="{@account1_name}" />
	<wait timeout="3" />

	<!-- Click on the EditView Pagination - Prev... -->
	<span class="pagination">
		<button index="1" />
	</span>
	<browser assert="{@account2_name}" />
	<wait timeout="3" />

	<button value="Cancel" alert="true" />

	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>


	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />

	<puts text="Test: Accounts_10264.xml finished." />
</soda>