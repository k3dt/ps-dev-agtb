<soda>
	<!--
		Quote record related to this account can be viewed by clicking "Subject" link
		on "QUOTES" sub-panel.
	-->
	<puts text="Test: Accounts_0429.xml starting." />

	<!--Create a account record-->
	<script file="{@global.scriptsdir}/modules/accounts/part2/Accounts_0078.xml" />

	<!--Search the record-->
	<link id="moduleTab_Accounts" />
	<browser assert="Accounts" />

	<textfield id="name_basic" set="{@pre_account_name}" />
	<button id="search_form_submit" />
	<table class="list view">
		<link text="{@pre_account_name}" />
	</table>


	<!--Go its detail view & click "edit"-->
	<browser assert="{@pre_account_name}" />


	<!--Go to Quote detail page-->
	<link text="{@pre_quote_subpanel_name}" />
	<browser assert="{@pre_quote_subpanel_name}" />


	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Quote_Delete_Lib.xml" />
	<puts text="Test: Accounts_0429.xml finished." />
</soda>
