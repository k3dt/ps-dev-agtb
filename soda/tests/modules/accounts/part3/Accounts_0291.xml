<soda>
	<puts text="Test: Accounts_0291.xml starting." />

	<!--Create a account record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_Lib.xml" />

	<!--Search the record-->
	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>
	<wait timeout="3" />
	<textfield id="name_basic" set="{@pre_account_name}" />
	<button id="search_form_submit" />
	<table class="list view">
		<link text="{@pre_account_name}" />
	</table>

	<!--Go its detail view-->
	<browser assert="{@pre_account_name}" />
	<button id="account_leads_select_button" />
	<textfield id="first_name_advanced" set="{@pre_lead_first_name}" />
	<textfield id="last_name_advanced" set="{@pre_lead_last_name}" />
	<button value="Search" />
	<wait timeout="5" />
	<link text="{@pre_lead_salutation} {@pre_lead_first_name} {@pre_lead_last_name}" />
	<wait timeout="3" />
	<div id="list_subpanel_leads">
		<link text="{@pre_lead_salutation} {@pre_lead_first_name} {@pre_lead_last_name}" click="false" />
		<link class="listViewTdToolsS1" index="2" alert="true" />
	</div>

	<wait timeout="5" />
	<div id="list_subpanel_leads">
		<span class="pageNumbers" assert="(0 - 0 of 0)" />
	</div>
	<link id="moduleTab_Leads" />
	<div class="moduleTitle">
		<link href="index.php?module=Leads&amp;action=index" click="false" />
	</div>
	<wait timeout="3" />
	<textfield id="search_name_basic" set="{@pre_lead_first_name} {@pre_lead_last_name}" />
	<button id="search_form_submit" />
	<wait timeout="5" />
	<table class="list view">
		<span class="pageNumbers" assert="(1 - 1 of 1)" />
	</table>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml" />
	<puts text="Test: Accounts_0291.xml finished." />
</soda>
