<soda>
	<puts text="Test: Accounts_0316.xml starting." />
	<timestamp />
	<!--Create a account record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.xml" />

	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>
	<wait timeout="3" />
	<textfield id="name_basic" set="{@pre_account_name}" />
	<button id="search_form_submit" />
	<table class="list view">
		<link text="{@pre_account_name}" />
	</table>
	<button id="accounts_contacts_create_button" />
	<wait timeout="3" />
	<csv file="{@global.scriptsdir}/csvs/accounts/Accounts_0316.csv" var="accounts0316" />
	<form id="form_SubpanelQuickCreate_Contacts">
		<textfield id="first_name" set="{@accounts0316.first_name}" />
		<textfield id="last_name" set="{@accounts0316.last_name}" />
	</form>
	<button id="Contacts_subpanel_save_button" />
	<wait timeout="5" />
	<div id="list_subpanel_contacts">
		<table class="list view">
			<row class="pagination">
				<browser assert="(1 - 1 of 1)" />
			</row>
		</table>
	</div>
	<div id="list_subpanel_contacts">
		<table class="list view">
			<link class="listViewTdToolsS1" index="2" alert="true" />
		</table>
	</div>
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Contact_Delete_Lib.xml" />
	<puts text="Test: Accounts_0316.xml finished." />
</soda>
