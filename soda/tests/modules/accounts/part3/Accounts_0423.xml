<soda>
	<!--
		Select _Verify that selecting a lead on an account detail view can be
		cancelled.
	-->
	<puts text="Test: Accounts_0423.xml starting." />

	<!--Create a account record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.xml" />

	<!--Search the record-->
	<link id="moduleTab_Accounts" />
	<browser assert="Accounts" />

	<textfield id="name_basic" set="{@pre_account_name}" />
	<button id="search_form_submit" />
	<table class="list view">
		<link text="{@pre_account_name}" />
	</table>

	<!--Go its detail view && click "Select"-->
	<browser assert="{@pre_account_name}" />
	<button id="account_leads_select_button" />
	<browser send_keys="Ctrl+W" />

	<div id="list_subpanel_leads">
		<span class="pageNumbers" assert="(0 - 0 of 0)" />
	</div>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<puts text="Test: Accounts_0423.xml finished." />
</soda>
