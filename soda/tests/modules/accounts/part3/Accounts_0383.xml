<soda>
	<puts text="Test: Accounts_0383.xml starting." />

	<!--Create a account record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.xml" />

	<!--Search the record-->
	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>
	<wait timeout="3" />
	<textfield id="name_basic" set="{@pre_account_name}" />
	<button id="search_form_submit" />
	<table class="list view">
		<link text="{@pre_account_name}" />
	</table>

	<!--Create Schedule Meeting-->
	<browser assert="{@pre_account_name}" />
	<button value="  Log Call  " />
	<csv file="{@global.scriptsdir}/csvs/accounts/Accounts_0214.csv" var="call_record_0383">
		<textfield id="name" set="{@call_record_0383.name} {@stamp}" />
		<textfield id="date_start_date" set="{@currentdate}" />
		<textfield id="description" set="{@call_record_0383.description}" />

		<select id="date_start_hours" set="{@call_record_0383.date_start_hours}" />
		<select id="date_start_minutes" set="{@call_record_0383.date_start_minutes}" />
		<select name="duration_minutes" set="{@call_record_0383.duration_minutes}" />
		<select id="status" set="Not Held" />

		<button id="Calls_subpanel_save_button" />
	</csv>
	<wait timeout="15" />
	<div id="list_subpanel_history">
		<span class="pageNumbers" assert="(0 - 0 of 0)" />
	</div>

	<div id="list_subpanel_activities">
		<span class="pageNumbers" assert="(1 - 1 of 1)" />
	</div>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Call_Delete_Lib.xml" />
	<puts text="Test: Accounts_0383.xml finished." />
</soda>
