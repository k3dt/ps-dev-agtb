<soda>
	<!-- Verify that "Scheduling" name is displayed as selected for scheduling a call -->
	<puts text="Test: Calendar_0822.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/calendar/CalendarAssertText.csv" var="calendar" />

	<!--Create a call record-->
	<script file="{@global.scriptsdir}/modules/lib/Call_Create_Lib.xml" />

	<div id="Calls">
		<link text="View Calls" />
	</div>
	<div class="moduleTitle">
		<link href="index.php?module=Calls&amp;action=index" click="false" />
	</div>

	<button id="search_form_clear" />
	<textfield id="name_basic" set="{@pre_call_name}" />
	<button id="search_form_submit" />

	<form id="MassUpdate">
		<table class="list view">
			<row class="oddListRowS1">
				<cell index="3">
					<link index="1" />
				</cell>
			</row>
		</table>
	</form>
	<browser assert="Scheduling" />

	<script file="{@global.scriptsdir}/modules/lib/Call_Delete_Lib.xml" />

	<puts text="Test: Calendar_0822.xml finished." />
</soda>