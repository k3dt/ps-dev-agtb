<soda>
	<!-- Verify that meeting can be scheduled at a specified time -->
	<puts text="Test: Calendar_0696.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/calendar/CalendarAssertText.csv" var="calendar" />
	<timestamp />

	<link id="moduleTab_Calendar" />
	<link text="09:00" />

	<table id="daily_cal_table">
		<div id="9_appt">
			<radio value="Meeting" click="true" />
			<textfield name="name" set="meeting_record{@stamp}" />
			<button name="button" />
		</div>
	</table>
	<div class="moduleTitle">
		<link href="index.php?module=Meetings&amp;action=index" click="false" />
		<browser assert="meeting_record{@stamp}" />
	</div>

	<puts text="Test: Calendar_0696.xml finished." />
</soda>