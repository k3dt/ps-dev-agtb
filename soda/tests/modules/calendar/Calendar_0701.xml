<soda>
	<!-- Verify that invitees can be added to a meeting -->
	<puts text="Test: Calendar_0701.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/calendar/CalendarAssertText.csv" var="calendar" />

	<!--Create a meeting record/-->
	<script file="{@global.scriptsdir}/modules/lib/Meeting_Create_Lib.xml" />

	<div id="Meetings">
		<link text="View Meetings" />
	</div>
	<div class="moduleTitle">
		<link href="index.php?module=Meetings&amp;action=index" click="false" />
	</div>

	<form id="MassUpdate">
		<table class="list view">
			<link text="{@pre_meeting_name}" />
		</table>
	</form>
	<div class="moduleTitle">
		<link href="index.php?module=Meetings&amp;action=index" click="false" />
		<browser assert="{@pre_meeting_name}" />
	</div>

	<button id="edit_button" />

	<!-- Add Invitees -->
	<div id="scheduler">
		<textfield name="search_last_name" set="qauser" />
		<button value="Search" />
		<div id="list_div_win">
			<button value="Add" />
		</div>
	</div>
	<button value="Save" />
    
	<div id="list_subpanel_users">
		<link text="qauser" exist="true" click="false" />
		<browser assert="(1 - 2 of 2)" />
	</div>

	<!--Delete the meeting record/-->
	<script file="{@global.scriptsdir}/modules/lib/Meeting_Delete_Lib.xml" />

	<puts text="Test: Calendar_0701.xml finished." />
</soda>