<soda>
   <puts text="KnowledgeBase_Createtag_Lib: Starting" />
   <timestamp />
   <link href="index.php?module=KBDocuments&amp;action=index" />
   <link text="Administer Knowledge Base" />
   <browser assert="Knowledge Base Admin" />
   <select id="action_id" set="Create New Tag" />
   <browser assert="Select the parent tag from tree" />
   <csv file="{@global.scriptsdir}/modules/lib/KnowledgeBase_Createtag_Lib.csv" 
      var="KB_New_tag">
      <div id="ygtv1">
         <link text="Tags" />
      </div>
      <textfield id="tag_new" set="{@KB_New_tag.tag} {@user.name} {@stamp}" />
      <button id="tag_save" />
      <link text="{@KB_New_tag.tag} {@user.name} {@stamp}(0)" click="false" />
   </csv>
   <var var="KB_tag" set="{@KB_New_tag.tag} {@user.name} {@stamp}(0)" />
   <!--puts var='{@KB_tag}(0)'/-->
   <puts text="KnowledgeBase_Createtag_Lib: Finished" />
</soda>
