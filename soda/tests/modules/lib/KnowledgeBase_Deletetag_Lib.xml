<soda>
   <puts text='KnowledgeBase_Deletetag_Lib: Starting' />
   <link id='moduleTab_KBDocuments'/>
   <link text='Administer Knowledge Base'/>
   <wait timeout='1'/>
   <browser assert='Administer Knowledge Base'/>
  
   <select id='action_id' set='Delete Tag'/>
   <browser assert='Select tag(s) to be deleted from tree'/>
   <wait timeout='1'/>
   <checkbox id='selected_tags[]' index='1' click='true' />
   <button id='tag_delete' alert='true' />
   <wait timeout='2'/>
   <div id='tagstree'>
       <link text='{@KB_tag}' click='false' exist='false' />
   </div>
   
   <puts text='KnowledgeBase_Deletetag_Lib: Finished' />
</soda>

