<soda>
<puts text="ReportTo_Lib: Starting" />
   <csv file="{@global.scriptsdir}/modules/lib/ReportTo_Lib.csv" var="ReportTo">
      <link text="Admin" />
      <link text="User Management" />
      <link text="{@ReportTo.last_name}" />
	  <button id='edit_button'/>

      <button name="btn1" />
      <browser assert="User Search" />
      <textfield id="last_name_advanced" set="{@ReportTo.admin_user}" />
      <button value="Search" />
      <link text="{@ReportTo.admin_user}" />
      <button id="Save" />
      <script file="{@global.scriptsdir}/modules/lib/Opp_Create_Lib.xml" />
      <!--Save the new user to a var-->
      <var var="report_user" set="{@ReportTo.last_name}" />
      <!--Login back to Admin-->
      <link text="Logout" />
      <script file="{@global.scriptsdir}/modules/lib/login.xml" />
   </csv>
   <puts text="ReportTo_Lib: Finished" />
</soda>

