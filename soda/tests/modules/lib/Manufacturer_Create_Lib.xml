   <link text="Manufacturers" />
   <timestamp />
   <button value="Create"/>
   <csv file="{@global.scriptsdir}/modules/lib/Manufacturer_Create_Lib.csv" 
      var="Manufacturer_Create_Lib">
      <textfield name="name" 
         set="{@Manufacturer_Create_Lib.name} {@user.name} {@stamp}" />
