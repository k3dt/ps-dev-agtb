<soda>
   <puts text="TeamMember_Opp_Lib: Starting" />
   <csv file="scripts/sugarcrm/modules/lib/TeamMember_Opp_Lib.csv" 
      var="TeamMember">
      <link text="Logout" />
      <textfield id="user_name" set="{@TeamMember.last_name}" />
      <textfield id="user_password" set="{@TeamMember.pass}" />
      <button id="login_button" />
      <browser assert="Welcome" />
      <browser assert="Server response time" />
      <script file="scripts/SugarCRM/modules/lib/Opp_Create_Lib.xml" />
         <var var="pre_user_name" set="{@TeamMember.last_name}" />
         <link text="Logout" />
      <script file="scripts/sugarcrm/modules/lib/login.xml" />
   </csv>
   <puts text="TeamMember_Opp_Lib: Finished" />
</soda>

