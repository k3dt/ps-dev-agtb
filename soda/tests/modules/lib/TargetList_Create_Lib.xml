<soda>
	<puts text="Lib: TargetList_Create_Lib.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaign" />

	<link href="index.php?module=ProspectLists&amp;action=index" />
	<div class="moduleTitle">
		<link href="index.php?module=ProspectLists&amp;action=index" click="false" />
	</div>

	<csv file="{@global.scriptsdir}/modules/lib/TargetList_Create_Lib.csv" 
      var="TargetList_New_Record">
		
		<link text="Create Target List" />
		<browser assert="Description" />

		<form id="EditView">
			<timestamp />
			<textfield name="name" 
            set="{@TargetList_New_Record.name} {@user.name} {@stamp}" /> 
			<select id="list_type" set="{@TargetList_New_Record.type}" />
			<textfield name="description" 
            set="{@TargetList_New_Record.description} " />
			<button value="Save" />
		</form>
		<div class="moduleTitle">
			<link href="index.php?module=ProspectLists&amp;action=index" click="false" />
			<browser assert="{@TargetList_New_Record.name} {@user.name} {@stamp}" />
		</div>

		<form name="DetailView">
			<hidden name="record" var="TargetList_Id" />
		</form>
	</csv>
	<var var="TargetList_Type" set="{@TargetList_New_Record.type}" />
	<var var="TargetList_Name" 
      set="{@TargetList_New_Record.name} {@user.name} {@stamp}" />

	<puts text="Lib: TargetList_Create_Lib.xml finished." />
</soda>