      var="TaxRate_Create_Lib">
      <link text="Admin" />
      <link text="Tax Rates" />
      <timestamp/>
      <button value="Create" />
      <textfield name="name" set="{@TaxRate_Create_Lib.name} {@stamp}" />
      <textfield name="value" set="{@TaxRate_Create_Lib.value}" />
      <select_list name="status" set="{@TaxRate_Create_Lib.status}" />
      <button value="Save" />
      <hidden name="record" var="taxrate_id" />
      <browser assert="{@TaxRate_Create_Lib.name} {@stamp}" />
      <var var="TaxRate_Create_Lib" set="{@TaxRate_Create_Lib.name} {@stamp}" />
   </csv>
   <puts text="TaxRate_Create_Lib: Finished" />
</soda>

