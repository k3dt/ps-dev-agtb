<soda>
    	<csv file="{@global.scriptsdir}/csvs/manufacturer/Manufacturer_560.csv" var="manufacturer_record_560">
        <puts text="---Manufacturer_Record_560.xml---Start!"/>
        <!--Manufacturer-Edit Verify that manufacture can be edited.-->
        <script file='{@global.scriptsdir}/modules/lib/Manufacturer_Create_Lib.xml'/>
        <link href="index.php?module=Administration&amp;action=index"/>
        <link text="Manufacturers"/>
        <table class="list view">
                <link text="{@Manufacturer_Create_Lib}"/> 
        </table>
        <table class="edit view">
            <textfield name="name" set="{@manufacturer_record_560.name} {@user.name} {@stamp}"/>
