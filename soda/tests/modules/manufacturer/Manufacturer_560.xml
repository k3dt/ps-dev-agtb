        </table>
             <button value="Save"/>
             <hidden name="record" var="manufacturer_id"/>
        <table class="list view">
                <browser assert="{@manufacturer_record_560.name} {@user.name} {@stamp}"/> 
        </table>
        <var var='Manufacturer_560_Name' set='{@manufacturer_record_560.name} {@user.name} {@stamp}'/>
    </csv>  
        <!--Run Delete XML-->
        <script file='{@global.scriptsdir}/modules/lib/Manufacturer_Delete_Lib.xml'/>
        <puts text="---Manufacturer_Record_560.xml---Finished!"/>   
</soda>
