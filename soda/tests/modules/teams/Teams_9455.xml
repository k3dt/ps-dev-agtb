<soda>
	<!-- Edit record-Remove all teams and cancel -->
	<puts text="Test: Teams_9455.xml starting." />
	<csv file="{@global.scriptsdir}/modules/lib/Document_Create_Lib.csv" var="Doc" />
	<csv file="{@global.scriptsdir}/csvs/teams/Teams_9455.csv" var="Teams_9455" />

	<script file="{@global.scriptsdir}/modules/lib/Document_Create_Lib.xml" />

	<button id="edit_button" />

	<!--Remove team -->
	<table id="EditView_team_name_table">
		<image id="remove_team_name_collection_0" click="true" />
	</table>
	<button value="Cancel" alert="true" />
	<div class="detail view">
		<browser assert="{@Doc_Name}" />
		<browser assert="{@Teams_9455.teamname}" />
	</div>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Document_Delete_Lib.xml" />

	<puts text="Test: Teams_9455.xml finished." />
</soda>