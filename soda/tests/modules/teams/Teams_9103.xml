<soda>
	<!-- Verify delete multiple teams -->
	<puts text="Test: Teams_9103.xml starting." />

	<!-- Create two teams. -->
	<script file="{@global.scriptsdir}/modules/lib/Team_Create_Lib.xml" />
	<var var="team_name1" set="{@pre_team_name}" />
	<var var="team_id1" set="{@pre_team_id}" />
	<script file="{@global.scriptsdir}/modules/lib/Team_Create_Lib.xml" />
	<var var="team_name2" set="{@pre_team_name}" />
	<var var="team_id2" set="{@pre_team_id}" />

	<!-- delete teams in list view -->
	<link text="Teams" />
	<browser assert="Teams" />

	<form id="search_form">
		<button id="search_form_clear" />	
		<button id="search_form_submit" />	
	</form>
	<table class="list view">
		<checkbox value="{@team_id1}" click="true" />
		<checkbox value="{@team_id2}" click="true" />
	</table>

	<link id="actions_link" />
	<link text="Delete" alert="true" />

	<form id="search_form">
		<button id="search_form_clear" />
		<textfield id="name_basic" set="{@team_name1}" />
		<button value="Search" />
	</form>

	<table class="list view">
		<span class="pageNumbers" assert="(0 - 0 of 0)" />
	</table>

	<form id="search_form">
		<button id="search_form_clear" />
		<textfield id="name_basic" set="{@team_name2}" />
		<button value="Search" />
	</form>
	<table class="list view">
		<span class="pageNumbers" assert="(0 - 0 of 0)" />
	</table>

	<!--clear search-->
	<form id="search_form">
		<button id="search_form_clear" />
		<button id="search_form_submit" />
	</form>

	<puts text="Test: Teams_9103.xml finished." />
</soda>