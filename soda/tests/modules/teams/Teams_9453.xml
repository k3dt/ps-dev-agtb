<soda>
	<!-- Edit record-Select only one team and cancel -->
	<puts text="Test: Teams_9453.xml starting." />
	<csv file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.csv" var="account" />

	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.xml" />

	<csv file="{@global.scriptsdir}/csvs/teams/Teams_9452.csv" var="teams_9453">
		<button id="edit_button" />

		<!--Teams select-->
		<table id="EditView_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />

		<textfield name="name" set="{@teams_9453.team1}" /> 
		<button value="Search" />

		<link text="{@teams_9453.team1}" />

		<button value="Cancel" alert="true" />           
	</csv>

	<table id="detailpanel_1">
		<browser assert="{@pre_account_name}" />        
	</table>
	<table id="detailpanel_3">
		<browser assert="{@account.team_name}" />
		<browser assert="{@teams_9453.team1}" exist="false" />
	</table>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_delete_Lib.xml" />

	<puts text="Test: Teams_9453.xml finished." />
</soda>