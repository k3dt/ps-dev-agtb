<soda>
	<!--
		Remove "Charcter Set" and "Copy to Sent Email" ad-hoc setting from Options
		panel for composed emails
	-->
	<puts text="Test: Emails_9679.xml starting." />
	<link id="moduleTab_Emails" />
	<wait timeout="15" />
	<button id="composeButton" />
	<wait timeout="10" />
	<table id="composeHeaderTable1">
		<button index="4" />
	</table>
	<div id="composeRightTabs1" >
    <browser assertnot="Character Set" />
    <browser assertnot="Copy to Sent Email" />
	</div>
	<puts text="Test: Emails_9679.xml finished." />
</soda> 
