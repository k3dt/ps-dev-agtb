<soda>
	<!--
		No reaction after clicking the Clear Form on Mail Account
		Properties window
	-->
	<puts text="Test: Emails_9901.xml starting." />

	<!--Nav to emails module-->
	<link id="moduleTab_Emails" />
	<wait timeout="3" />
	<button id="settingsButton" />
	<link text="Mail Accounts" />
	<wait timeout="1" />
	<button id="addButton" />
	<!--
		Enter information then Clear Form, Clear Form has an id of addButton not a
		typo
	-->
	<csv file="{@global.scriptsdir}/modules/lib/Email_Acct_Create_Lib.csv" var="ClearForm" />
	<textfield id="ie_name" set="{@ClearForm.ie_name}" />
	<textfield id="email_user" set="{@ClearForm.email_user}" />
	<button id="addButton" />
	<textfield name="ie_name" assert="" />
	<textfield name="email_user" assert="" />
  <div id="editAccountDialogue" >
     <link text="Close" />
	</div>
	<div id="settingsDialog">
		<link text="Close" />
	</div>

	<puts text="Test: Emails_9901.xml finished" />
</soda>