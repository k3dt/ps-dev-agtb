<soda>
	<!-- Create contact_Verify that duplicating contact can be canceled. -->
	<puts text="Test: Contacts_1450.xml starting" />
	<timestamp />
	<!--Create contact record -->
	<script file="{@global.scriptsdir}/modules/lib/Contact_Create_Lib.xml" />

	<!--Search the Contact record -->
	<link id="moduleTab_Contacts" />
	<div class="moduleTitle">
		<link href="index.php?module=Contacts&amp;action=index" click="false" />
	</div>
	<button id="search_form_clear" />
	<textfield id="search_name_basic" set="{@pre_contact_first_name}" />
	<button id="search_form_submit" />
	<link text="{@pre_contact_first_name} {@pre_contact_last_name}" />
	<wait timeout="5" />
	
	<!--Go its detail view $ "Duplicate" -->
	<div class="moduleTitle">
		<browser assert="{@pre_contact_first_name} {@pre_contact_last_name}" />
	</div>
	
	<button id="duplicate_button" />
	<wait timeout="3" />
	<csv file="{@global.scriptsdir}/csvs/contacts/Contacts_1449.csv" var="contact_record_1450">
		<!--user select -->
		<button id="btn_assigned_user_name" />
		<browser assert="User Search" />
		<textfield id="last_name_advanced" set="{@contact_record_1450.assigned_user_name}" />
		<button value="Search" />
		<link text="{@contact_record_1450.assigned_user_name}" />

		<!--teams select -->
		<table id="EditView_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@contact_record_1450.team_name}" />
		<button value="Search" />
		<link text="{@contact_record_1450.team_name}" />

		<!--fill the fields and "Cancel" -->
		<form id="EditView">
			<textfield name="first_name"
				set="{@contact_record_1450.first_name} {@user.name} {@stamp}" />
			<textfield id="last_name" set="{@contact_record_1450.last_name}" />
			<textfield id="title" set="{@contact_record_1450.title}" />
			<textfield id="phone_work" set="{@contact_record_1450.phone_work}" />
			<textfield id="phone_mobile" set="{@contact_record_1450.phone_mobile}" />
			<textfield id="phone_fax" set="{@contact_record_1450.phone_fax}" />
			<textfield id="description" set="{@contact_record_1450.description}" />
			<textfield id="Contacts0emailAddress0" set="{@contact_record_1450.emailAddress0}" />
			<select id="lead_source" set="{@contact_record_1450.lead_source}" />
			<checkbox id="do_not_call" set="{@contact_record_1450.do_not_call}" />
			<checkbox id="sync_contact" set="{@contact_record_1450.sync_contact}" />
			<textfield id="primary_address_street" set="{@contact_record_1450.primary_address_street}" />
			<textfield id="primary_address_city" set="{@contact_record_1450.primary_address_city}" />
			<textfield id="primary_address_state" set="{@contact_record_1450.primary_address_state}" />
			<textfield id="primary_address_postalcode" set="{@contact_record_1450.primary_address_postalcode}" />
			<textfield id="primary_address_country" set="{@contact_record_1450.primary_address_country}" />
			<textfield id="alt_address_street" set="{@contact_record_1450.alt_address_street}" />
			<textfield id="alt_address_city" set="{@contact_record_1450.alt_address_city}" />
			<textfield id="alt_address_state" set="{@contact_record_1450.alt_address_state}" />
			<textfield id="alt_address_postalcode" set="{@contact_record_1450.alt_address_postalcode}" />
			<textfield id="alt_address_country" set="{@contact_record_1450.alt_address_country}" />
			<button value="Cancel" alert="true" />
		</form>
	</csv>

	<browser assert="{@pre_contact_first_name} {@pre_contact_last_name}" />
	<script file="{@global.scriptsdir}/modules/lib/Contact_Delete_Lib.xml" />
	<puts text="Test: Contacts_1450 finished" />
</soda>

