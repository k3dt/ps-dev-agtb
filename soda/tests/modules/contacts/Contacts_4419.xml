<soda>
	<!-- Select lead_Verify that a related lead can be selected into a contact from 
		contact detail view. -->

	<puts text="Test: Contacts_4419.xml starting." />

	<script file="{@global.scriptsdir}/modules/lib/Contact_Create_lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_lib.xml" />

	<link id="moduleTab_Contacts" />
	<div class="moduleTitle">
		<link href="index.php?module=Contacts&amp;action=index" click="false" />
	</div>
	<wait timeout="3" />

	<textfield id="search_name_basic" set="{@pre_contact_first_name} {@pre_contact_last_name}" />
	<button id="search_form_submit" />
	<table class="list view">
		<link text="{@pre_contact_first_name} {@pre_contact_last_name}" />
	</table>
	<wait timeout="3" />

	<div id="list_subpanel_leads">
		<button id="contact_leads_select_button" />
	</div>
	<wait timeout="3" />

	<table class="list view">
		<link text="{@pre_lead_salutation} {@pre_lead_first_name} {@pre_lead_last_name}" />
	</table>

	<div id="list_subpanel_leads">
		<link text="{@pre_lead_salutation} {@pre_lead_first_name} {@pre_lead_last_name}"
			exist="true" click="false" />
	</div>

	<script file="{@global.scriptsdir}/modules/lib/Contact_Delete_lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_lib.xml" />

	<puts text="Test: Contacts_4419.xml finished." />
	<puts text=" " />
</soda>
