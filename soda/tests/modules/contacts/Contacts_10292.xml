<soda>
	<!--
		Action:
			a)    Login Sugar CRM as a valid user
			b)    Go to a module detail view (such as one that contains "Cases") at first time
			c)    Try to select a case by click "Select" button in Case subpanel
			d)    Click "Clear" button on pop-up select window 
		Result:
			 Nothing happens.
	-->


	<puts text="Test: Contacts_10292.xml starting." />

	<script file="{@global.scriptsdir}/modules/lib/Case_Create_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Contact_Create_Lib.xml" />
	
	<button id="contacts_cases_select_button" />

	<browser assert="{@pre_case_name}" />
	<textfield id="name_advanced" assert=""/>

	<button value="Clear" />

	<browser assert="{@pre_case_name}" />
	<textfield id="name_advanced" assert="" />
	
	<link text="{@pre_case_name}" />
	
	
	<script file="{@global.scriptsdir}/modules/lib/Contact_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Case_Delete_Lib.xml" />
	
	<puts text="Test: Contacts_10292.xml finished." />
</soda>