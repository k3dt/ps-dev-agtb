<soda>
	<puts text="Test: Forecast_1783.xml starting." />

	<script file="{@global.scriptsdir}/modules/lib/Opp_Create_Lib.xml" />

	<csv file="{@global.scriptsdir}/csvs/forecast/Forecast_1783.csv" var="Forecast_1783">
		<link id="moduleTab_Forecasts" />

		<table id="forecastsWorksheet">       
			<textfield id="commit_best_case" set="{@Forecast_1783.commit_best_case}" />
			<textfield id="commit_likely_case" set="{@Forecast_1783.commit_likely_case}" />
			<textfield id="commit_worst_case" set="{@Forecast_1783.commit_worst_case}" />    
			<button id="btn_commit" alert="true" />
			<browser assert="No Active time periods for Forecasting." />
		</table>
		<var var="Pre_Forecast_Record" set="{@Forecast_1783.name} {@user.name} {@stamp}" />
	</csv>

	<!-- Delete test data. -->
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Opp_Delete_Lib.xml" />

	<puts text="Test: Forecast_1783.xml finished." />
</soda>