<soda>
	<puts text="Test: Forecast_1782.xml starting." />

	<script file="{@global.scriptsdir}/modules/lib/Opp_Create_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/TimePeriod_Create_Lib.xml" />

	<csv file="{@global.scriptsdir}/csvs/forecast/Forecast_1782.csv" var="Forecast_1782">
		<link id="moduleTab_Forecasts" />

		<form id="CommitEditView">
			<textfield id="commit_best_case" set="{@Forecast_1782.commit_best_case}" />
			<textfield id="commit_likely_case" set="{@Forecast_1782.commit_likely_case}" />
			<textfield id="commit_worst_case" set="{@Forecast_1782.commit_worst_case}" />
		</form>
		<button id="btn_commit" alert="true" />

		<var var="Pre_Forecast_Record" set="{@Forecast_1782.name} {@user.name} {@stamp}" />
	</csv>

	<!-- Delete test data. -->
	<script file="{@global.scriptsdir}/modules/lib/TimePeriod_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Opp_Delete_Lib.xml" />

	<puts text="Test: Forecast_1782.xml finished." />
</soda>