<soda>
	<!--
		Mass Update Leads_Verify that the selected leads can be updated by changing
		all the fields in 'Mass Update' sub-panel.
	-->
	<puts text="Test: Leads_1899.xml starting" />

	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_Lib.xml">
	</script>

	<link id="moduleTab_Leads" />
	<div class="moduleTitle">
		<link href="index.php?module=Leads&amp;action=index" click="false" />
	</div>

	<!--Select a lead record-->
	<textfield id="search_name_basic" set="{@pre_lead_first_name}" />
	<button id="search_form_submit" />

	<csv file="{@global.scriptsdir}/csvs/leads/Leads_1897.csv" var="Leads_1897">
		<checkbox name="massall" click="true" />

		<!-- Open massupdate panel-->
		<link id="actions_link" />
		<link text="Mass Update" />

		<!--Select assigned user-->
		<table id="mass_update_table">
			<button value="Select" />
		</table>
		<browser assert="User Search" />
		<textfield id="user_name_advanced" set="admin" />
		<button value="Search" />
		<link text="Administrator" />

		<table id="mass_update_table">
			<select id="mass_do_not_call" set="{@Leads_1897.mass_do_not_call}" />
			<select id="mass_converted" set="{@Leads_1897.mass_converted}" />
			<select id="mass_lead_source" set="{@Leads_1897.mass_lead_source}" />
			<select name="optout_primary" set="{@Leads_1897.optout_primary}" />
			<select id="mass_status" set="{@Leads_1897.mass_status}" />
		</table>

		<!--Select a team record-->
		<table id="MassUpdate_team_name_table">
			<button value="Select" />
		</table>
		<link text="Global" />

		<button id="update_button" alert="true" />

		<div class="moduleTitle">
			<link href="index.php?module=Leads&amp;action=index" click="false" />
		</div>
		<textfield id="search_name_basic" set="{@pre_lead_first_name}" />
		<button id="search_form_submit" />
		<link
			text="{@Lead_New_Record.salutation} {@pre_lead_first_name} {@pre_lead_last_name}" />


		<browser
			assert="{@Lead_New_Record.salutation} {@pre_lead_first_name} {@pre_lead_last_name}" />
		<browser assert="Administrator" />
		<!--browser assert = "Global"/-->
		<browser assert="{@Leads_1897.mass_lead_source}" />
		<browser assert="{@Leads_1897.mass_status}" />
		<!--checkbox name = "do_not_call" assert = "{@Leads_1892.do_not_call}"/-->
		<!--Converted no such field in detail view-->
		<!--No optout_primary-->

	</csv>
	<!--Delete the record-->

	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml">
	</script>
	<puts text="Test: Leads_1899.xml finished" />
</soda>
