<soda>
	<!--
		Schedule Meeting By Full Form_Verify that the meeting is not scheduled for
		lead when using "Cancel" function
	-->
	<puts text="Test: Leads_1933.xml starting" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_Lib.xml" />

	<!--go to the leads-->
	<link id="moduleTab_Leads" />
	<browser assert="Leads" />
	<button id="search_form_clear" />
	<textfield id="search_name_basic" set="{@pre_lead_first_name}" />
	<button id="search_form_submit" />
	<link text="{@pre_lead_salutation} {@pre_lead_first_name} {@pre_lead_last_name}" />

	<!--create a meeting-->
	<div id="list_subpanel_activities">
		<table class="list view">
			<button value="  Schedule Meeting  " />
		</table>
	</div>
	<button id="Meetings_subpanel_full_form_button" />
	<csv file="{@global.scriptsdir}/csvs/leads/Leads_1933.csv" var="Leads_1933">
		<textfield id="name" set="{@Leads_1933.name}" />
		<textfield id="location" set="{@Leads_1933.location}" />
		<textfield id="description" set="{@Leads_1933.description}" />
		<select id="status" set="{@Leads_1933.status}" />

		<textfield id="date_start_date" set="{@Leads_1933.date_start_date}" />
		<select id="date_start_hours" set="{@Leads_1933.date_start_hours}" />
		<select id="date_start_minutes" set="{@Leads_1933.date_start_minutes}" />

		<textfield name="duration_hours" set="{@Leads_1933.duration_hours}" />
		<select name="duration_minutes" set="{@Leads_1933.duration_minutes}" />

		<checkbox name="reminder_checked" set="{@Leads_1933.reminder_checked}" />

		<!--user select-->
		<button id="btn_assigned_user_name" />
		<browser assert="User Search" />
		<textfield name="last_name_advanced" set="{@Leads_1933.assigned_user_name}" />
		<button value="Search" />
		<link text="{@Leads_1933.assigned_user_name}" />

		<!--teams select-->
		<table id="EditView_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@Leads_1933.team_name}" />
		<button value="Search" />
		<link text="{@Leads_1933.team_name}" />
		<button value="Cancel" alert="true" />
	</csv>
	<wait timeout="6" />
	<!--verify the meeting record-->
	<div id="list_subpanel_activities">
		<span class="pageNumbers" assert="(0 - 0 of 0)" />
	</div>

	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml" />
	<puts test="Test: Leads_1933.xml finished" />
</soda>
