<soda>
	<!--
		Remove Scheduled Meeting_Verify that meeting can be canceled removing from the
		lead detail view when using "rem" function
	-->
	<puts text="Test: Leads_1937.xml starting" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_Lib.xml" />

	<!--go to the leads-->
	<link id="moduleTab_Leads" />
	<div class="moduleTitle">
		<link href="index.php?module=Leads&amp;action=index" click="false" />
	</div>
	<button id="search_form_clear" />
	<textfield id="search_name_basic" set="{@pre_lead_first_name}" />
	<button id="search_form_submit" />
	<link text="{@pre_lead_salutation} {@pre_lead_first_name} {@pre_lead_last_name}" />

	<!--create a meeting-->
	<div id="list_subpanel_activities">
		<table class="list view">
			<button value="  Schedule Meeting  " />
		</table>
	</div>
	<button id="Meetings_subpanel_full_form_button" />
	<csv file="{@global.scriptsdir}/csvs/leads/Leads_1937.csv" var="Leads_1937">
		<textfield id="name" set="{@Leads_1937.name}" />
		<textfield id="location" set="{@Leads_1937.location}" />
		<textfield id="description" set="{@Leads_1937.description}" />
		<select id="status" set="{@Leads_1937.status}" />

		<textfield id="date_start_date" set="{@Leads_1937.date_start_date}" />
		<select id="date_start_hours" set="{@Leads_1937.date_start_hours}" />
		<select id="date_start_minutes" set="{@Leads_1937.date_start_minutes}" />

		<textfield name="duration_hours" set="{@Leads_1937.duration_hours}" />
		<select name="duration_minutes" set="{@Leads_1937.duration_minutes}" />

		<checkbox name="reminder_checked" set="{@Leads_1937.reminder_checked}" />

		<!--user select-->
		<button id="btn_assigned_user_name" />
		<browser assert="User Search" />
		<textfield name="last_name_advanced" set="{@Leads_1937.assigned_user_name}" />
		<button value="Search" />
		<link text="{@Leads_1937.assigned_user_name}" />

		<!--teams select-->
		<table id="EditView_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@Leads_1937.team_name}" />
		<button value="Search" />
		<link text="{@Leads_1937.team_name}" />
		<button value="Save" />
	</csv>
	<wait timeout="5" />
	<!--verify the meeting record-->
	<div id="list_subpanel_activities">
		<link class="listViewTdToolsS1" index="2" alert="false" />
	</div>
	<wait timeout="5" />
	<div id="list_subpanel_activities">
		<span class="pageNumbers" assert="(1 - 1 of 1)" />
	</div>

	<script file="{@global.scriptsdir}/modules/lib/Meeting_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml" />
	<puts text="Test: Leads_1937.xml finished" />
</soda>
