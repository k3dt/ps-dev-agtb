<soda>
	<!--
		Edit Scheduled Meeting_Verify that editing scheduled meeting related to a lead
		can be canceled
	-->
	<puts text="Test: Leads_1935.xml starting" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_Lib.xml" />

	<!--go to the leads-->
	<link id="moduleTab_Leads" />
	<browser assert="Leads" />
	<button id="search_form_clear" />
	<textfield id="search_name_basic" set="{@pre_lead_first_name}" />
	<button id="search_form_submit" />
	<link text="{@pre_lead_salutation} {@pre_lead_first_name} {@pre_lead_last_name}" />

	<!--create a meeting-->
	<div id="list_subpanel_activities">
		<table class="list view">
			<button value="  Schedule Meeting  " />
		</table>
	</div>
	<button id="Meetings_subpanel_full_form_button" />
	<csv file="{@global.scriptsdir}/csvs/leads/Leads_1935.csv" var="Leads_1935">
		<textfield id="name" set="{@Leads_1935.name}" />
		<textfield id="location" set="{@Leads_1935.location}" />
		<textfield id="description" set="{@Leads_1935.description}" />
		<select id="status" set="{@Leads_1935.status}" />

		<textfield id="date_start_date" set="{@Leads_1935.date_start_date}" />
		<select id="date_start_hours" set="{@Leads_1935.date_start_hours}" />
		<select id="date_start_minutes" set="{@Leads_1935.date_start_minutes}" />

		<textfield name="duration_hours" set="{@Leads_1935.duration_hours}" />
		<select name="duration_minutes" set="{@Leads_1935.duration_minutes}" />

		<checkbox name="reminder_checked" set="{@Leads_1935.reminder_checked}" />

		<!--user select-->
		<button id="btn_assigned_user_name" />
		<browser assert="User Search" />
		<textfield name="last_name_advanced" set="{@Leads_1935.assigned_user_name}" />
		<button value="Search" />
		<link text="{@Leads_1935.assigned_user_name}" />

		<!--teams select-->
		<table id="EditView_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@Leads_1935.team_name}" />
		<button value="Search" />
		<link text="{@Leads_1935.team_name}" />
		<button value="Save" />
	</csv>
	<wait timeout="6" />
	<!--verify the meeting record-->
	<div id="list_subpanel_activities">
		<link class="listViewTdToolsS1" index="1" />
	</div>
	<textfield id="name" set="{@Leads_1935.new_name}" />
	<button value="Cancel" alert="true" />
	<div id="list_subpanel_activities">
		<table class="list view">
			<link text="{@Leads_1935.name}" />
		</table>
	</div>


	<script file="{@global.scriptsdir}/modules/lib/Meeting_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml" />
	<puts test="Test: Leads_1935.xml finished" />
</soda>
