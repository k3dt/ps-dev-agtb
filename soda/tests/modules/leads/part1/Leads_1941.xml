<soda>
	<!--
		Edit Scheduled Call_Verify that editing scheduled call related to a lead can
		be canceled
	-->
	<puts text="Test: Leads_1941.xml starting." />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_Lib.xml" />

	<!--go to the leads-->
	<link id="moduleTab_Leads" />
	<browser assert="Leads" />
	<button id="search_form_clear" />
	<textfield id="search_name_basic" set="{@pre_lead_first_name}" />
	<button id="search_form_submit" />
	<link text="{@pre_lead_salutation} {@pre_lead_first_name} {@pre_lead_last_name}" />

	<!--create a call-->
	<div id="list_subpanel_activities">
		<table class="list view">
			<button value="  Log Call  " />
		</table>
	</div>
	<csv file="{@global.scriptsdir}/csvs/leads/Leads_1941.csv" var="Leads_1941">
		<textfield id="name" set="{@Leads_1941.name}" />
		<textfield id="date_start_date" set="{@Leads_1941.date_start_date}" />
		<select id="date_start_hours" set="{@Leads_1941.date_start_hours}" />
		<select id="date_start_minutes" set="{@Leads_1941.date_start_minutes}" />

		<textfield name="duration_hours" set="{@Leads_1941.duration_hours}" />
		<select name="duration_minutes" set="{@Leads_1941.duration_minutes}" />

		<select id="direction" set="{@Leads_1941.direction}" />
		<select id="status" set="{@Leads_1941.status}" />

		<checkbox name="reminder_checked" set="{@Leads_1941.reminder_checked}" />
		<textfield id="description" set="{@Leads_1941.description}" />

		<!--user select-->
		<button id="btn_assigned_user_name" />
		<browser assert="User Search" />
		<textfield name="last_name_advanced" set="{@Leads_1941.assigned_user_name}" />
		<button value="Search" />
		<link text="{@Leads_1941.assigned_user_name}" />

		<!--teams select-->
		<table id="form_SubpanelQuickCreate_Calls_team_name_table">
			<button value="Select" />
		</table>
		<wait timeout="5" />
		<browser assert="Team Search" />
		<textfield name="name" set="{@Leads_1941.team_name}" />
		<button value="Search" />
		<wait timeout="10" />
		<link text="{@Leads_1941.team_name}" />
		<button value="Save" />
	</csv>
	<wait timeout="20" />
	<!--modify the call record-->
	<div id="list_subpanel_activities">
		<link class="listViewTdToolsS1" index="1" />
	</div>

	<wait />
	<textfield id="name" set="{@Leads_1941.new_name}" />
	<button value="Cancel" alert="true" />
	<div id="list_subpanel_activities">
		<table class="list view">
			<link text="{@Leads_1941.name}" />
		</table>
	</div>

	<script file="{@global.scriptsdir}/modules/lib/Call_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml" />
	<puts test="Test: Leads_1941.xml finished." />
</soda>
