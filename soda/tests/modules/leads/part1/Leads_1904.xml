<soda>
	<!--
		Search Leads_Verify that leads can be searched using basic search function.
	-->
	<puts text="Test: Leads_1904.xml starting" />

	<!--Create three records and save its counter-->

	<script
		file="{@global.scriptsdir}/modules/leads/module_lib/Leads_search_data_prepare.xml" />


	<link id="moduleTab_Leads" />
	<div class="moduleTitle">
		<link href="index.php?module=Leads&amp;action=index" click="false" />
	</div>

	<csv file="{@global.scriptsdir}/csvs/leads/Leads_1904.csv" var="Leads_1904">
		<!--Basic search by first name-->
		<button id="search_form_clear" />
		<link id="basic_search_link" required="false" />
		<textfield id="search_name_basic" set="{@Leads_1904.first_name}" />
		<button id="search_form_submit" />

		<span class='pageNumbers' assert='(1 - {@Leads_1904.counter} of {@Leads_1904.counter})' />

	</csv>
	<!--Delete the record-->

	<script
		file='{@global.scriptsdir}/modules/leads/module_lib/Lead_Delete_search_data.xml' />
	<puts text="Test: Leads_1904.xml finished" />
</soda>
