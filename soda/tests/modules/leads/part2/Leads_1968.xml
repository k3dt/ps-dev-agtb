<soda>
	<!--
		In-Line Create Note_Verify that note can be canceled for in-line creating from
		History sub-panel for a lead
	-->
	<puts text="Test: Leads_1968.xml starting." />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_Lib.xml" />

	<!--go to the leads-->
	<link id="moduleTab_Leads" />
	<div class="moduleTitle">
		<link href="index.php?module=Leads&amp;action=index" click="false" />
	</div>
	<button id="search_form_clear" />
	<textfield id="search_name_basic" set="{@pre_lead_first_name}" />
	<button id="search_form_submit" />
	<link text="{@pre_lead_salutation} {@pre_lead_first_name} {@pre_lead_last_name}" />

	<!--create a note-->
	<div id="list_subpanel_history">
		<table class="list view">
			<button value="  Create Note or Attachment  " />
		</table>
	</div>
	<csv file="{@global.scriptsdir}/csvs/leads/Leads_1968.csv" var="Leads_1968">
		<div id="form_SubpanelQuickCreate_Notes_tabs">
			<wait timeout="3" />
			<textfield id="name" set="{@Leads_1968.name}" />
			<textfield id="description" set="{@Leads_1968.description}" />
		</div>

		<!--teams select-->
		<table id="form_SubpanelQuickCreate_Notes_team_name_table">
			<span class="id-ff multiple ownline">
				<button value="Select" />
			</span>
		</table>


		<browser assert="Team Search" />
		<textfield name="name" set="{@Leads_1968.team_name}" />
		<button value="Search" />
		<link text="{@Leads_1968.team_name}" />
		<button id="Notes_subpanel_cancel_button" alert="true" />
	</csv>
	<wait timeout="5" />


	<!--verify the note-->
	<div id="list_subpanel_history">
		<span class="pageNumbers" assert="(0 - 0 of 0)" />
	</div>

	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml" />
	<puts test="Test: Leads_1968.xml finished." />
</soda>
