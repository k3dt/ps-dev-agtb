<soda>
	<!--
		Lead Detail View_Verify that information of contact converted from lead can be
		viewed by clicking the name link of the contact at the top left corner of lead
		detail view
	-->
	<puts text="Test: Leads_1985.xml starting." />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_Lib_FULL.xml" />
	<!--go to the leads-->
	<link id="moduleTab_Leads" />
	<div class="moduleTitle">
		<link href="index.php?module=Leads&amp;action=index" click="false" />
	</div>
	<link text="Basic Search" required="false" />
	<button id="search_form_clear" />
	<textfield id="search_name_basic" set="{@pre_lead_first_name}" />
	<button id="search_form_submit" />
	<link text="Mr. {@pre_lead_first_name} {@pre_lead_last_name}" />
	<button value="Convert Lead" />
	<button value="Save" />
	<link id="moduleTab_Contacts" />
	<div class="moduleTitle">
		<link href="index.php?module=Contacts&amp;action=index" click="false" />
	</div>
	<textfield id="search_name_basic" set="{@pre_lead_first_name}" />
	<button id="search_form_submit" />
	<wait timeout="20" />
	<link text="{@pre_lead_salutation} {@pre_lead_first_name} {@pre_lead_last_name}" />
	<script file="{@global.scriptsdir}/modules/lib/Contact_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml" />
	<puts text="Test: Leads_1985.xml finished." />
</soda>
