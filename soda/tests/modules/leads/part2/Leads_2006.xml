<soda>
	<!-- Convert Lead_Verify that lead can be converted with a newly scheduled call -->
	<puts text="Test: Leads_2006.xml starting" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_Lib_FULL.xml" />
	<!--go to the leads -->
	<link id="moduleTab_Leads" />
	<div class="moduleTitle">
		<link href="index.php?module=Leads&amp;action=index" click="false" />
	</div>
	<wait timeout="5" />
	<button id="search_form_clear" />
	<textfield id="search_name_basic" set="{@pre_lead_first_name}" />
	<button id="search_form_submit" />
	<link text="Mr. {@pre_lead_first_name} {@pre_lead_last_name}" />
	<csv file="{@global.scriptsdir}/csvs/leads/Leads_2006.csv" var="Leads_2006">
		<!--Convert Lead -->
		<button value="Convert Lead" />
		<checkbox name="newCalls" set="true" />
		<textfield name="Callsname" set="{@Leads_2006.Appointmentsname}" />
		<textfield name="Callsdescription" set="{@Leads_2006.Appointmentsdescription}" />
		<button value="Save" />
		<!--verify -->
		<link text="Mr. {@pre_lead_first_name} {@pre_lead_last_name}" />
		<wait timeout="5" />
		<div id="subpanel_activities">
			<table class="list view">
				<span class="pageNumbers" assert="(1 - 1 of 1)" />
			</table>
		</div>


	</csv>
	<script file="{@global.scriptsdir}/modules/lib/Call_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Contact_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml" />
	<puts text="Test: Leads_2006.xml starting" />
</soda>
