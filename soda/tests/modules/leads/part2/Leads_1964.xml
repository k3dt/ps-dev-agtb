<soda>
	<!--
		In-Line Create Meeting_Verify that meeting can be canceled for in-line
		creating from Activities sub-panel for a lead
	-->
	<puts text="Test: Leads_1964.xml starting" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_Lib.xml" />

	<!--go to the leads-->
	<link id="moduleTab_Leads" />
	<div class="moduleTitle">
		<link href="index.php?module=Leads&amp;action=index" click="false" />
	</div>
	<button id="search_form_clear" />
	<textfield id="search_name_basic" set="{@pre_lead_first_name}" />
	<button id="search_form_submit" />
	<link text="{@pre_lead_salutation} {@pre_lead_first_name} {@pre_lead_last_name}" />

	<!--create a meeting-->
	<div id="list_subpanel_activities">
		<table class="list view">
			<button value="  Schedule Meeting  " />
		</table>
	</div>
	<csv file="{@global.scriptsdir}/csvs/leads/Leads_1964.csv" var="Leads_1964">
		<textfield id="name" set="{@Leads_1964.name}" />
		<textfield id="location" set="{@Leads_1964.location}" />
		<textfield id="description" set="{@Leads_1964.description}" />
		<select id="status" set="{@Leads_1964.status}" />

		<textfield id="date_start_date" set="{@Leads_1964.date_start_date}" />
		<select id="date_start_hours" set="{@Leads_1964.date_start_hours}" />
		<select id="date_start_minutes" set="{@Leads_1964.date_start_minutes}" />

		<textfield name="duration_hours" set="{@Leads_1964.duration_hours}" />
		<select name="duration_minutes" set="{@Leads_1964.duration_minutes}" />

		<checkbox name="reminder_checked" set="{@Leads_1964.reminder_checked}" />

		<!--user select-->
		<button id="btn_assigned_user_name" />
		<browser assert="User Search" />
		<textfield name="last_name_advanced" set="{@Leads_1964.assigned_user_name}" />
		<button value="Search" />
		<link text="{@Leads_1964.assigned_user_name}" />

		<!--teams select-->
		<table id="form_SubpanelQuickCreate_Meetings_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@Leads_1964.team_name}" />
		<button value="Search" />
		<link text="{@Leads_1964.team_name}" />
		<button id="Meetings_subpanel_cancel_button" alert="true" />
	</csv>
	<wait timeout="5" />


	<!--verify the meeting-->
	<div id="list_subpanel_activities">
		<span class="pageNumbers" assert="(0 - 0 of 0)" />
	</div>

	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml" />
	<puts text="Test: Leads_1964.xml finished" />
</soda>
