<soda>
	<!--
		Convert Lead_Verify that an opportunity can be created when converting a lead
		with an related account created
	-->
	<puts text="Test: Leads_2005.xml starting" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_Lib.xml" />
	<!--go to the leads-->
	<link id="moduleTab_Leads" />
	<div class="moduleTitle">
		<link href="index.php?module=Leads&amp;action=index" click="false" />
	</div>
	<button id="search_form_clear" />
	<textfield id="search_name_basic" set="{@pre_lead_first_name}" />
	<button id="search_form_submit" />
	<link text="Mr. {@pre_lead_first_name} {@pre_lead_last_name}" />
	<csv file="{@global.scriptsdir}/csvs/leads/Leads_2005.csv" var="Leads_2005">
		<!--Convert Lead-->
		<button value="Convert Lead" />
		<textfield name="Accountsname" set="{@Leads_2005.Accountsname}" />
		<checkbox name="newOpportunities" set="true" />
		<textfield name="Opportunitiesname" set="{@Leads_2005.Opportunitiesname}" />
		<textfield id="Opportunitiesdate_closed" set="{@Leads_2005.Opportunitiesjscal_field}" />
		<select name="Opportunitiessales_stage" set="{@Leads_2005.Opportunitiessales_stage}" />
		<textfield name="Opportunitiesamount" set="{@Leads_2005.Opportunitiesamount}" />
		<textfield name="Opportunitiesdescription" set="{@Leads_2005.Opportunitiesdescription}" />
		<button value="Save" />
		<!--verify-->
		<link text="{@Leads_2005.Opportunitiesname}" />
		<browser assert="{@Leads_2005.Opportunitiesname}" />
		<browser assert="{@Leads_2005.Opportunitiesjscal_field}" />
		<browser assert="{@Leads_2005.Opportunitiessales_stage}" />
		<browser assert="{@Leads_2005.Opportunitiesamount}" />
		<browser assert="{@Leads_2005.Opportunitiesdescription}" />


	</csv>
	<script file="{@global.scriptsdir}/modules/lib/Opp_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Contact_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml" />
	<puts text="Test: Leads_2005.xml finished" />
</soda>
