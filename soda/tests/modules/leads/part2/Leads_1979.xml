<soda>
	<!--
		Update Lead_Verify that "Other Address" information can be copied to "Primary
		Address" by clicking the second arrow button between these two address areas
	-->
	<puts text="Test: Leads_1979.xml starting" />
	<!--create a leads-->
	<link id="moduleTab_Leads" />
	<div class="moduleTitle">
		<link href="index.php?module=Leads&amp;action=index" click="false" />
	</div>
	<link text="Create Lead" />
	<csv file="{@global.scriptsdir}/csvs/leads/Leads_1979.csv" var="lead_1979">
		<textfield id="last_name" set="{@lead_1979.last_name}" />
		<select id="status" set="{@lead_1979.New}" />
		<button value="Save" />
		<browser assert="New" />
		<button value="Edit" />
		<select id="status" set="{@lead_1979.Assigned}" />
		<button value="Save" />
		<browser assert="Assigned" />
		<button value="Edit" />
		<select id="status" set="{@lead_1979.In_Process}" />
		<button value="Save" />
		<browser assert="In Process" />
		<button value="Edit" />
		<select id="status" set="{@lead_1979.Converted}" />
		<button value="Save" />
		<browser assert="Converted" />
		<button value="Edit" />
		<select id="status" set="{@lead_1979.Recycled}" />
		<button value="Save" />
		<browser assert="Recycled" />
		<button value="Edit" />
		<select id="status" set="{@lead_1979.Dead}" />
		<button value="Save" />
		<browser assert="Dead" />
	</csv>
	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml" />
	<puts text="Test: Leads_1979.xml finished" />
</soda>
