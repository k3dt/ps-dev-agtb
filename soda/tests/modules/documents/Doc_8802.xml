<soda>
	<!-- Documents_edit_document -->
	<puts text="Test: Doc_8802.xml starting." />

	<script file="{@global.scriptsdir}/modules/lib/Document_Create_Lib.xml" />

	<link id="moduleTab_Documents" />
	<browser assert="Documents" />

	<button name="clear" />
	<textfield id="document_name_basic" set="{@Doc_Name}" />
	<button id="search_form_submit" />

	<link text="{@Doc_Name}" />

	<browser assert="Documents" />

	<button id="edit_button" />
	<csv file="{@global.scriptsdir}/csvs/documents/document_8802.csv" var="Doc_8802">
		<textfield name="document_name" set="{@Doc_8802.document_name} {@user.name} {@stamp}" /> 
		<select id="category_id" set="{@Doc_8802.category}" />
		<select id="template_type" set="{@Doc_8802.template_type}" />
		<select id="subcategory_id" set="{@Doc_8802.subcategory}" />
		<textfield name="active_date" set="{@Doc_8802.active_date}" />
		<textfield name="exp_date" set="{@Doc_8802.exp_date}" />         
		<textfield name="description" set="{@Doc_8802.description}" />
		<button value="Save" />

		<browser assert="{@Doc_8802.document_name} {@user.name} {@stamp}" />
		<browser assert="{@Doc_8802.attach_path}" />
		<browser assert="{@Doc_8802.revision}" /> 
		<browser assert="{@Doc_8802.category}" />
		<browser assert="{@Doc_8802.template_type}" />
		<browser assert="{@Doc_8802.subcategory}" />
		<browser assert="{@Doc_8802.status_id}" />
		<browser assert="{@Doc_8802.active_date}" />
		<browser assert="{@Doc_8802.exp_date}" />         
		<browser assert="{@Doc_8802.description}" /> 
	</csv>

	<script file="{@global.scriptsdir}/modules/lib/Document_Delete_Lib.xml" />

	<puts text="Test: Doc_8802.xml finished." />
</soda>
