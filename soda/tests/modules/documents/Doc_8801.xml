	<browser assert="{@Doc_Name}" />
	<csv file="{@global.scriptsdir}/modules/lib/Document_Create_Lib.csv" var="Doc_8801">
		<browser assert="{@Doc_8801.attach_path}" />
		<browser assert="{@Doc_8801.revision}" /> 
		<browser assert="{@Doc_8801.category}" />
		<browser assert="{@Doc_8801.template_type}" />
		<browser assert="{@Doc_8801.subcategory}" />
		<browser assert="{@Doc_8801.status_id}" />
		<browser assert="{@Doc_8801.active_date}" />
		<browser assert="{@Doc_8801.exp_date}" />         
		<browser assert="{@Doc_8801.description}" />
	</csv>

	<script file="{@global.scriptsdir}/modules/lib/Document_Delete_Lib.xml" />

	<puts text="Test: Doc_8801.xml finished." />
</soda>
