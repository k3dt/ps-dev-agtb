<soda>
	<!-- Campaign Wizard_Verify the required fields in Campaign Header -->
	<puts text="Test: Campaigns_1177.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaigns" />

	<link id="moduleTab_Campaigns" />
	<div class="moduleTitle">
		<link href="index.php?module=Campaigns&amp;action=index" click="false" />
	</div>

	<link text="Create Campaign (Wizard)" />
	<csv file="{@global.scriptsdir}/csvs/campaigns/Campaigns_1166.csv" var="Campaign1177">
		<timestamp />
		<radio value="1" set="true" />
		<button id="startbutton" />
		<browser assert="Campaign Header" />
		<!--teams select-->            
		<table id="wizform_team_name_table">
			<textfield id="team_name_collection_0" set="" />
		</table>
		<button id="wiz_next_button" />
		<browser assert="Missing required field: Name:" />
		<browser assert="Missing required field: End Date:" />
		<!--other fields-->
		<textfield id="name" set="{@Campaign1177.name} {@stamp}" />
		<textfield id="end_date" set="{@Campaign1177.end_date}" />
		<button id="wiz_next_button" />
		<browser assert="No Primary Team specified" />

		<table id="wizform_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@Campaign1177.teamname}" /> 
		<button value="Search" />
		<link text="{@Campaign1177.teamname}" />
		<radio value="1" set="true" />
		<button id="wiz_next_button" />
		<browser assert="Campaign Budget" />
	</csv>

	<puts text="Test: Campaigns_1177.xml finished." />
</soda>