<soda>
	<!-- Add contact to a target list -->
	<puts text="Test: Campaigns_9566.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaigns" />

	<!-- Create a target list and a contact. -->
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Create_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Contact_Create_Lib.xml" />

	<link id="moduleTab_Contacts" />

	<checkbox name="massall" set="true" />
	<link text="Actions" />
	<link text="Add To Target List" />
	<browser assert="Target Lists Search" />

	<link text="{@TargetList_Name}" />
	<div class="moduleTitle">
		<browser assert="{@campaigns.contacts_main}" />
	</div>

	<link id="moduleTab_Campaigns" />
	<div class="moduleTitle">
		<link href="index.php?module=Campaigns&amp;action=index" click="false" />
	</div>

	<link text="View Target Lists" />
	<div class="moduleTitle">
		<link href="index.php?module=ProspectLists&amp;action=index" click="false" />
	</div>

	<link text="{@TargetList_Name}" />

	<link text="{@pre_contact_first_name} {@pre_contact_last_name}" />

	<!--Delete the test data-->
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Contact_Delete_Lib.xml" />

	<puts text="Test: Campaigns_9566.xml finished." />
</soda>