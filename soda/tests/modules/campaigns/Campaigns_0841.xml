<soda>
	<!-- Create a target list -->
	<puts text="Test: Campaigns_0841.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaign" />

	<link id="moduleTab_ProspectLists" />
    <div class="moduleTitle">
        <link href="index.php?module=ProspectLists&amp;action=index" click="false" />
    </div>


	<csv file="{@global.scriptsdir}/csvs/campaigns/Campaigns_0841.csv" var="targetlist0841">
		<link text="Create Target List" />
		<div class="moduleTitle">
			<link href="index.php?module=ProspectLists&amp;action=index" click="false" />
			<browser assert="{@campaign.target_lists_create}" />
		</div>

		<form id="EditView">
			<timestamp />

			<textfield name="name" set="{@targetlist0841.name} {@user.name} {@stamp}" /> 
			<select id="list_type" set="{@targetlist0841.type}" />
			<textfield name="description" set="{@targetlist0841.description} " />
			<button value="Save" />
		</form>
		<browser assert="{@targetlist0841.name} {@user.name} {@stamp}" />
	</csv>

	<script file="{@global.scriptsdir}/modules/lib/TargetList_Delete_Lib.xml" />

	<puts text="Test: Campaigns_0841.xml finished." />
</soda>