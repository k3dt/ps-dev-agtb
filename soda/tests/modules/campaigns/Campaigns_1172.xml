<soda>
	<!-- Campaign Wizard_Verify that newsletter can be created with existing target lists -->
	<puts text="Test: Campaigns_1172.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaigns" />

	<link id="moduleTab_ProspectLists" />
	<div class="moduleTitle">
		<link href="index.php?module=ProspectLists&amp;action=index" click="false" />
	</div>

	<link text="Create Target List" />
	<form id="EditView">
		<textfield name="name" set="T2-UnSubscription" /> 
		<select id="list_type" set="Suppression List - By Domain" />
		<button value="Save" />
	</form>

	<link text="Create Target List" />
	<form id="EditView">
		<textfield name="name" set="T3-Test" /> 
		<select id="list_type" set="Test" />
		<button value="Save" />
	</form>

	<csv file="{@global.scriptsdir}/csvs/campaigns/Campaigns_1166.csv" var="Campaign1172">
		<timestamp />

		<link id="moduleTab_Campaigns" />
		<link text="Create Campaign (Wizard)" />
		<radio value="1" set="true" />
		<button id="startbutton" />
		<browser assert="Campaign Header" />
		<!--teams select-->            
		<table id="wizform_team_name_table">
			<textfield id="team_name_collection_0" set="" />
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@Campaign1172.teamname}" /> 
		<button value="Search" />
		<link text="{@Campaign1172.teamname}" />
		<radio value="1" set="true" />

		<!--user select-->
		<div id="step1">
			<button value="Select" />
		</div>
		<browser assert="User Search" />                        
		<textfield name="last_name_advanced" set="{@Campaign1172.assignto}" />                        
		<button value="Search" />
		<link text="{@Campaign1172.assignto}" />

		<!--other fields-->
		<textfield id="name" set="{@Campaign1172.name} {@stamp}" />
		<select id="status" set="{@Campaign1172.status}" />
		<select id="frequency" set="{@Campaign1172.frequency}" />
		<textfield id="end_date" set="{@Campaign1172.end_date}" />
		<textfield id="wiz_content" set="{@Campaign1172.wiz_content}" />

		<button id="wiz_next_button" />
		<browser assert="Campaign Budget" />
		<textfield id="budget" set="{@Campaign1172.budget}" />
		<textfield id="actual_cost" set="{@Campaign1172.actual_cost}" />
		<textfield id="expected_revenue" set="{@Campaign1172.expected_revenue}" />
		<textfield id="expected_cost" set="{@Campaign1172.expected_cost}" />
		<select id="currency_id" set="{@Campaign1172.currency_id}" />
		<textfield id="objective" set="{@Campaign1172.objective}" />

		<button id="wiz_next_button" />
		<browser assert="Campaign Tracker URLs" />
		<textfield id="tracker_name" set="{@Campaign1172.tracker_name}" />
		<textfield id="tracker_url" set="{@Campaign1172.tracker_url}" />
		<wait timeout="2" />
		<div id="tracker_input_div">
			<button value="Create Tracker" />
		</div>
		<checkbox id="is_optout1" set="false" />

		<button id="wiz_next_button" />
		<browser assert="Subscription Information" />
		<button id="wiz_step3_unsubscription_name_button" />
		<link text="T2-UnSubscription" />
		<button id="wiz_step3_test_name_button" />
		<link text="T3-Test" />

		<button id="wiz_submit_finish_button" />
		<button value="Edit Campaign" click="false" />
		<button value="View Status" click="false" />
		<button value="View ROI" click="false" />       
		<link text="{@Campaign1172.tracker_name}" click="false" />
		<link text="{@Campaign1172.name} {@stamp} Subscription List" click="false" />    
		<link text="T2-UnSubscription" click="false" />
		<link text="T3-Test" click="false" />
	</csv>

	<!--delete the test data-->
	<script file="{@global.scriptsdir}/modules/lib/Campaign_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Delete_Lib.xml" />

	<puts text="Test: Campaigns_1172.xml finished." />
</soda>