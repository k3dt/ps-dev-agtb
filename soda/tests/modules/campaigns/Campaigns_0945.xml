<soda>
	<!-- Create a tracker URLs with "Opt-out" Link -->
	<puts text="Test: Campaigns_0945.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaign" />

	<!-- create a campaign record -->
	<script file="{@global.scriptsdir}/modules/lib/Campaign_Create_Lib.xml" />

	<form id="formcampaign_campaigntrakers">
		<button name="CampaignTrackers_create_button" />
	</form>

	<csv file="{@global.scriptsdir}/csvs/campaigns/Campaigns_0945.csv" var="Campaign0945">
		<checkbox id="is_optout" set="true" />
		<textfield name="tracker_name" set="{@Campaign0945.tracker_name}" />
		<button value="  Save  " />
	</csv>

	<link text="Create Lead Form" click="false" />
	<browser assert="{@Campaign0945.tracker_url}" />

	<script file="{@global.scriptsdir}/modules/lib/Campaign_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Delete_Lib.xml" />

	<puts text="Test: Campaigns_0945.xml finished." />
</soda>