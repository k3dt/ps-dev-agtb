<soda>
	<!-- Delete a target record -->
	<puts text="Test: Campaigns_0847.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaign" />

	<!--Create a targetlist record-->
	<script file="{@global.scriptsdir}/modules/lib/Target_Create_Lib.xml" />

	<!--Delete the record-->
	<button value="Delete" alert="true" />

	<!--Search the record-->
	<link text="View Targets" />
	<div class="moduleTitle">
		<link href="index.php?module=Prospects&amp;action=index" click="false" />
	</div>

	<!-- Clear the search conditions -->
	<button id="search_form_clear" />
	<textfield id="search_name_basic" set="{@Target_Last_Name}" />
	<button id="search_form_submit" />

	<form id="MassUpdate">
		<span class="pageNumbers" assert="(0 - 0 of 0)" />
	</form>

	<puts text="Test: Campaigns_0847.xml finished." />
</soda>