<soda>
	<!-- Target List - Duplicate Target List -->
	<puts text="Test: Campaigns_0977.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaigns" />

	<!--Create a targetlist record-->
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Create_Lib.xml" />

	<button id="duplicate_button" />
	<form id="EditView">
		<button value="Save" />
	</form>
	<browser assert="{@TargetList_Name}" />

	<!-- Clear the search conditions -->
	<link text="View Target Lists" />
	<browser assert="My Items" />
	<div id="content">
		<button name="clear" />
		<textfield id="name_basic" type="text" set="{@TargetList_Name}" />
		<button class="button" type="submit" value="Search" />
	</div>

	<form id="MassUpdate">
		<span class="pageNumbers" assert="(1 - 2 of 2)" />
	</form>

	<script file="{@global.scriptsdir}/modules/lib/TargetList_Delete_Lib.xml" />

	<puts text="Test: Campaigns_0977.xml finished." />
</soda>