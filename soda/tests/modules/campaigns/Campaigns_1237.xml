<soda>         
	<!-- Lead search function in the "Leads" sub-panel of targetlist detailview -->
	<puts text="Test: Campaigns_1237.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaigns" />

	<!--Create a lead and target list record-->
	<script file="{@global.scriptsdir}/modules/lib/Lead_Create_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Create_Lib.xml" />

	<!--Target select window-->
	<button id="prospect_list_leads_select_button" />

	<textfield id="first_name_advanced" set="{@pre_lead_first_name}" />
	<textfield id="last_name_advanced" set="{@pre_lead_last_name}" />
	<select name="lead_source_advanced[]" set="{@Lead_New_Record.lead_source}" />
	<button value="Search" />

	<link text="{@pre_lead_salutation} {@pre_lead_first_name} {@pre_lead_last_name}" />

	<!--Delete the test data-->
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml" />

	<puts text="Test: Campaigns_1237.xml finished." />
</soda>