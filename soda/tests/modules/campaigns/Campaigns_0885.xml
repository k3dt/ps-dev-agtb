<soda>
	<!-- Cancel the campaign record creation -->
	<puts text="Test: Campaigns_0885.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaign" />

	<link id="moduleTab_Campaigns" />
	<div class="moduleTitle">
		<link href="index.php?module=Campaigns&amp;action=index" click="false" />
	</div>

	<link text="Create Campaign (Classic)" />
	<csv file="{@global.scriptsdir}/csvs/campaigns/Campaigns_0885.csv" var="Campaign0885">
		<form id="EditView">
			<textfield name="name" set="{@Campaign0885.name} {@user.name} {@stamp}" /> 
			<select id="status" set="{@Campaign0885.status}" />
			<select id="campaign_type" set="{@Campaign0885.type}" />
			<textfield id="content" set="{@Campaign0885.description} " />    
			<textfield id="end_date" set="{@Campaign0885.enddate} " />              
			<button value="Cancel" alert="true" />
		</form>
		<div class="moduleTitle">
			<link href="index.php?module=Campaigns&amp;action=index" click="false" />
		</div>
	</csv>

	<!--Basic Search-->
	<button id="search_form_clear" />
	<textfield id="name_basic" set="{@Campaign0885.name} {@user.name} {@stamp}" />
	<button id="search_form_submit" />

	<form id="MassUpdate">
		<span class="pageNumbers" assert="(0 - 0 of 0)" />
	</form>

	<script file="{@global.scriptsdir}/modules/lib/TargetList_Delete_Lib.xml" />

	<puts text="Test: Campaigns_0885.xml finished." />
</soda>