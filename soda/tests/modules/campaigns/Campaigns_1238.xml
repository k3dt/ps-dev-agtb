<soda>
	<!-- User search function in the "Users" sub-panel of targetlist detailview -->
	<puts text="Test: Campaigns_1238.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaigns" />

	<!--Create a target list record-->
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Create_Lib.xml" />

	<!--Target select window-->
	<button id="prospect_list_users_select_button" />

	<csv file="{@global.scriptsdir}/modules/lib/SugarInit_Lib.csv" var="Campaigns_1238">
		<textfield name="last_name_advanced" set="{@Campaigns_1238.last_name}" />
		<textfield name="user_name_advanced" set="{@Campaigns_1238.last_name}" />
		<button value="Search" />
		<link text="{@Campaigns_1238.last_name}" />
	</csv>

	<!--Delete the test data-->
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Delete_Lib.xml" />

	<puts text="Test: Campaigns_1238.xml finished." />
</soda>