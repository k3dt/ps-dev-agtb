<soda>
	<!-- Delete a target list -->
	<puts text="Test: Campaigns_0843.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaign" />

	<!--Create a targetlist record-->
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Create_Lib.xml" />

	<!--Delete the record-->
	<button class="button" type="submit" value="Delete" alert="true" />

	<!--Search the record-->
	<link text="View Target Lists" />
	<div class="moduleTitle">
		<link href="index.php?module=ProspectLists&amp;action=index" click="false" />
	</div>

	<button id="search_form_clear" />
	<textfield name="name_basic" set="{@TargetList_Name}" />
	<button id="search_form_submit" />

	<span class="pageNumbers" assert="(0 - 0 of 0)" />

       <script file="{@global.scriptsdir}/modules/lib/TargetList_Delete_Lib.xml" />
	<puts text="Test: Campaigns_0843.xml finished." />
</soda>