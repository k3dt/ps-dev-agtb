<soda>
	<!-- Tacker URLs management_Verify the Rem link -->
	<puts text="Test: Campaigns_0954.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaign" />

	<!-- create a campaign record -->
	<script file="{@global.scriptsdir}/modules/lib/Campaign_Create_Lib.xml" />

	<div id="list_subpanel_tracked_urls">
		<button name="CampaignTrackers_create_button" />
	</div>

	<csv file="{@global.scriptsdir}/csvs/campaigns/Campaigns_0954.csv" var="Campaign0954">
		<checkbox id="is_optout" set="false" />
		<textfield name="tracker_name" set="{@Campaign0954.tracker_name}" />
		<textfield id="tracker_url" set="{@Campaign0954.tracker_url}" /> 
		<button value="  Save  " />
	</csv>

	<link text="Create Lead Form" click="false" />
	<browser assert="{@Campaign0954.tracker_url}" />
        
	<div id="list_subpanel_tracked_urls">
		<link class="listViewTdToolsS1" index="2" alert="true" />
                <wait timeout="5" />
		<link text="{@Campaign0954.tracker_name}" click="false" exist="false" />
	</div>

	<script file="{@global.scriptsdir}/modules/lib/Campaign_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Delete_Lib.xml" />

	<puts text="Test: Campaigns_0954.xml finished." />
</soda>