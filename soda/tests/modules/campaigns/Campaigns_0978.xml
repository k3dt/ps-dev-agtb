<soda>
	<!-- Delete Target List_Verify that "Cancel" function -->
	<puts text="Test: Campaigns_0978.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaigns" />

	<!--Create a targetlist record-->
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Create_Lib.xml" />

	<div id="list_subpanel_prospects">
		<button name="prospect_list_prospects_create_button" />
	</div>

	<csv file="{@global.scriptsdir}/csvs/campaigns/Campaigns_0978.csv" var="target0978">
		<!--teams select-->
		<table id="form_SubpanelQuickCreate_Prospects_team_name_table">
			<textfield name="team_name_collection_0" set="" /> 
			<button value="Select" />
		</table>
		<textfield name="name" set="{@target0978.teamname}" /> 
		<button value="Search" />
		<link text="{@target0978.teamname}" />

		<!--user select-->
		<div id="LBL_PROSPECT_INFORMATION">
			<button id="btn_assigned_user_name" />
		</div>
		<browser assert="User Search" />                        
		<textfield name="last_name_advanced" set="{@target0978.assignto}" />                        
		<button value="Search" />
		<link text="{@target0978.assignto}" />

		<div id="LBL_PROSPECT_INFORMATION">
			<timestamp />
			<textfield id="first_name" set="{@target0978.first_name}" /> 
			<textfield id="last_name" set="{@target0978.last_name} {@user.name} {@stamp}" /> 
			<textfield id="title" set="{@target0978.title}" /> 
			<textfield id="department" set="{@target0978.department}" />
			<checkbox id="do_not_call" set="true" />
			<textfield name="phone_work" set="{@target0978.phone_work}" /> 
			<textfield name="phone_mobile" set="{@target0978.phone_mobile}" /> 
			<textfield name="phone_fax" set="{@target0978.phone_fax}" />
		</div>

		<textfield name="Prospects0emailAddress0" set="{@target0978.email}" />
		<form id="form_SubpanelQuickCreate_Prospects">
			<button name="Prospects_subpanel_save_button" />
		</form>
	</csv>

	<link text="{@target0978.first_name} {@target0978.last_name} {@user.name} {@stamp}" click="false" />

	<!--delete the test data-->
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Target_Delete_Lib.xml" />

	<puts text="Test: Campaigns_0978.xml finished." />
</soda>