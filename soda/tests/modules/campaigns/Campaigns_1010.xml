<soda>
	<!-- that "Edit" function in "Targets" sub-panel -->
	<puts text="Test: Campaigns_1010.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/campaigns/CampaignAssertText.csv" var="campaigns" />

	<!-- Create a target and a targetlist. -->
	<script file="{@global.scriptsdir}/modules/lib/Target_Create_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Create_Lib.xml" />

	<div id="list_subpanel_prospects">
		<button id="prospect_list_prospects_select_button" />
	</div>

	<link text="{@Target_First_Name} {@Target_Last_Name}" />
	<div class="moduleTitle">
		<link href="index.php?module=ProspectLists&amp;action=index" click="false" />
	</div>
	<link text="{@Target_First_Name} {@Target_Last_Name}" click="false" />

	<div id="list_subpanel_prospects">
		<link class="listViewTdToolsS1" index="1" />
	</div>

	<div id="LBL_PROSPECT_INFORMATION">
		<timestamp />
		<textfield id="last_name" set="{@Target_New_Record.last_name} {@stamp}" /> 
	</div>
	<button value="Save" />
	<link text="{@Target_First_Name} {@Target_New_Record.last_name} {@stamp}" click="false" />

	<!--delete the test data-->
	<script file="{@global.scriptsdir}/modules/lib/TargetList_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Target_Delete_Lib.xml" />

	<puts text="Test: Campaigns_1010.xml finished." />
</soda>