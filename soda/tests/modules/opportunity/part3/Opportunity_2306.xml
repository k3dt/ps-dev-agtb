<soda>
	<puts text="Test: Opportunity_2306.xml starting" />
	<csv file="{@global.scriptsdir}/csvs/opportunity/Opportunity_2306.csv" var="Opportunity_2306">
		
		<link id="moduleTab_Opportunities" />
		<browser assert="Opportunities" />
		<button id="search_form_clear" />
		<form id="search_form">
			<textfield id="name_basic" set="{@pre_opp_name}" />
		</form>
		<button id="search_form_submit" />

		<table class="list view">
			<browser assert="{@pre_opp_name}" />
			<link text="{@pre_opp_name}" />
		</table>

		<table id="detailpanel_1">
			<browser assert="{@pre_opp_name}" />
		</table>

		<div id="list_subpanel_quotes">
			<button name="quotes_opportunities_select_button" />
		</div>
		<textfield name="name_advanced" set="{@pre_Quote_name}" />
		<button value="Search" />
		<link text="{@pre_Quote_name}" />
		<div id="list_subpanel_quotes">
			<table class="list view">
				<link text="{@pre_Quote_name}" click="false" />
				<link class="listViewTdToolsS1" index="1" />
			</table>
		</div>
		<wait timeout="3" />
		<!--User select-->
		<button id="btn_assigned_user_name" />
		<browser assert="User Search" />
		<textfield name="last_name_advanced" set="{@Opportunity_2306.assigned_user_name}" />
		<button value="Search" />
		<link text="{@Opportunity_2306.assigned_user_name}" />

		<!--Teams select-->
		<table id="EditView_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@Opportunity_2306.team_name}" />
		<button value="Search" />
		<link text="{@Opportunity_2306.team_name}" />

		<!--Opportunity select-->
		<button id="btn_opportunity_name" />
		<browser assert="Opportunity Search" />
		<textfield name="name_advanced" set="{@pre_opp_name}" />
		<button value="Search" />
		<table class="list view">
			<link text="{@pre_opp_name}" />
		</table>

		<!--Billing Account select-->
		<button id="btn_billing_account_name" />
		<browser assert="Account Search" />
		<textfield name="name_advanced" set="{@pre_account_name}" />
		<button value="Search" />
		<table class="list view">
			<link text="{@pre_account_name}" />
		</table>

		<!--Shipping Account select-->
		<button id="btn_shipping_account_name" />
		<browser assert="Account Search" />
		<textfield name="name_advanced" set="{@pre_account_name}" />
		<button value="Search" />
		<table class="list view">
			<link text="{@pre_account_name}" />
		</table>

		<!--Contact select-->
		<button id="btn_billing_contact_name" />
		<browser assert="Contact Search" />
		<textfield name="first_name_advanced" set="{@pre_contact_first_name}" />
		<button value="Search" />
		<table class="list view">
			<link text="{@pre_contact_first_name} {@pre_contact_last_name}" />
		</table>
		<form id="EditView">
			<textfield id="name"
				set="{@Opportunity_2306.quote_name_edit} {@user.name} {@stamp}" />
			<textfield id="purchase_order_num" set="{@Opportunity_2306.quote_purchase_order_num_edit}" />
			<select id="payment_terms" set="{@Opportunity_2306.quote_payment_terms_edit}" />
			<select id="quote_stage" set="{@Opportunity_2306.quote_quote_stage_edit}" />
			<textfield id="date_quote_expected_closed"
				set="{@Opportunity_2306.quote_date_quote_expected_closed_edit}" />
			<textfield id="original_po_date" set="{@Opportunity_2306.quote_original_po_date_edit}" />

			<select name="currency_id" set="{@Opportunity_2306.quote_currency_id_edit}" />
			<select name="shipper_id" set="{@Opportunity_2306.quote_shipper_id_edit}" />
			<select name="taxrate_id" set="{@TaxRate_Create_Lib}" />
			<checkbox id="calc_grand_total" click="true" />
			<checkbox id="show_line_nums" click="true" />
			<textfield name="description" set="{@Opportunity_2306.quote_description_edit}" />
			<button value="Save" name="button" />
		</form>
		<div id="list_subpanel_quotes">
			<table class="list view">
				<browser assert="{@Opportunity_2306.quote_name_edit} {@user.name} {@stamp}"
					click="false" />
			</table>
		</div>
		<div id="list_subpanel_quotes">
			<table class="list view">
				<link class="listViewTdToolsS1" index="1" />
			</table>
		</div>
		<wait timeout="3" />
		<form id="EditView">
			<textfield id="name" set="{@pre_Quote_name}" />
			<button value="Save" name="button" />
		</form>
		<div id="list_subpanel_quotes">
			<table class="list view">
				<link text="{@pre_Quote_name}" click="false" />
				<link class="listViewTdToolsS1" index="2" alert="true" />
				<wait timeout="3" />
				<link text="{@pre_Quote_name}" click="false" exist="false" />
			</table>
		</div>
	</csv>
	<puts text="Test: Opportunity_2306.xml finished" />
</soda>
