<soda>
        <csv file='{@global.scriptsdir}/csvs/opportunity/Opportunity_2295.csv' var='Opportunity_2295'>
        <puts text='----Opportunity_2295.xml--------Start!'/>
        <link id='moduleTab_Opportunities'/>
        <browser assert='Opportunities'/>
        <button id='search_form_clear'/>
        <form id='search_form'>
            <textfield id='name_basic' set='{@pre_opp_name}'/>
        </form>
        <button id='search_form_submit'/>
        
        <table class='list view'>
            <browser assert='{@pre_opp_name}'/>
            <link text='{@pre_opp_name}'/>
        </table>
        
        <table id='detailpanel_1'>
            <browser assert='{@pre_opp_name}'/>
        </table>
        
        <form id='formformNotes'>
            <button value='  Create Note or Attachment  '/>
        </form>         

        <!--teams select-->
        <table id='form_SubpanelQuickCreate_Notes_team_name_table'>
            <button value='Select'/>
        </table>
        <browser assert='Team Search'/>
        <textfield name='name' set='{@Opportunity_2295.team_name}'/> 
        <button value='Search'/>
        <link text='{@Opportunity_2295.team_name}'/>
            
        <form id='form_SubpanelQuickCreate_Notes'>
            <textfield id='name' set='{@Opportunity_2295.note_name} {@user.name} {@stamp}'/>
            <textarea id='description' set='{@Opportunity_2295.note_description}'/>           
            <button id='Notes_subpanel_cancel_button' alert="true"/>
        </form>
         
        <div id='list_subpanel_history'>
           <table class='list view'>             
             <browser assert='{@Opportunity_2295.note_name} {@user.name} {@stamp}' click='false' exist='false'/>
           </table>
        </div>
    </csv>
    <!--Run Delete XML-->
    <script file='{@global.scriptsdir}/modules/lib/Note_Delete_Lib.xml'/>
    <puts text='---Opportunity_2295.xml---Finished!'/>
</soda>
