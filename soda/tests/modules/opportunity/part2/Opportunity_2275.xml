<soda>
        <csv file='{@global.scriptsdir}/csvs/opportunity/Opportunity_2275.csv' var='Opportunity_2275'>
        <puts text='----Opportunity_2275.xml--------Start!'/>
        <link id='moduleTab_Opportunities'/>
        <browser assert='Opportunities'/>

        <button id='search_form_clear'/>
        <form id='search_form'>
            <textfield id='name_basic' set='{@pre_opp_name}'/>
        </form>
        <button id='search_form_submit'/>
        
        <table class='list view'>
            <browser assert='{@pre_opp_name}'/>
            <link text='{@pre_opp_name}'/>
        </table>
        
        <table id='detailpanel_1'>
            <browser assert='{@pre_opp_name}'/>
        </table>
        
         <div id='list_subpanel_activities'>
             <table class='list view'>
               <button value='  Create Task  '/>
         </table>
         </div>
         
         <form id='form_SubpanelQuickCreate_Tasks'>
             <button id='Tasks_subpanel_full_form_button'/> 
         </form>
         <timestamp/> 
         <form id='EditView'>
                <textfield id='name' set='{@Opportunity_2275.task_name} {@user.name} {@stamp}'/>
                <select id='priority' set='{@Opportunity_2275.task_priority}'/>
                <select id='status' set='{@Opportunity_2275.task_status}'/>
                <textfield id='description' set='{@Opportunity_2275.task_description}'/>

                <!--Due Date-->
                <textfield id='date_due_date' set='{@Opportunity_2275.task_date_due_date}'/>
                <select id='date_due_hours' set='{@Opportunity_2275.task_date_due_hours}'/>
                <select id='date_due_minutes' set='{@Opportunity_2275.task_date_due_minutes}'/>
                <!--select id='date_due_meridiem' set='{@Opportunity_2275.task_date_due_meridiem}'/-->
                <checkbox id='date_due_flag' click='true'/>

                <!--Start Date-->
                <textfield id='date_start_date' set='{@Opportunity_2275.task_date_start_date}'/>
                <select id='date_start_hours' set='{@Opportunity_2275.task_date_start_hours}'/>
                <select id='date_start_minutes' set='{@Opportunity_2275.task_date_start_minutes}'/>
                <!--select id='date_start_meridiem' set='{@Opportunity_2275.task_date_start_meridiem}'/-->
                <checkbox id='date_start_flag' click='true'/>

        </form>         
        <!--teams select-->
        <table id='EditView_team_name_table'>
            <button value='Select'/>
        </table>
        <browser assert='Team Search'/>
        <textfield name='name' set='{@Opportunity_2275.team_name}'/> 
        <button value='Search'/>
        <link text='{@Opportunity_2275.team_name}'/>
         <form id='EditView'>       
             <button value='Save'/>
         </form>
         
       <div id='list_subpanel_activities'>
           <table class='list view'>
             <browser assert='{@Opportunity_2275.task_name} {@user.name} {@stamp}'/>
           </table>
       </div>                                           
    </csv>
    <!--Run Delete XML-->
    <script file='{@global.scriptsdir}/modules/lib/Task_Delete_Lib.xml'/>
    <puts text='---Opportunity_2275.xml---Finished!'/>
</soda>
