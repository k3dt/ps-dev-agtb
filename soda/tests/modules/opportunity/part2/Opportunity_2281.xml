<soda>
        <csv file='{@global.scriptsdir}/csvs/opportunity/Opportunity_2281.csv' var='Opportunity_2281'>
        <puts text='----Opportunity_2281.xml--------Start!'/>
        <link id='moduleTab_Opportunities'/>
        <browser assert='Opportunities'/>

        <button id='search_form_clear'/>
        <form id='search_form'>
            <textfield id='name_basic' set='{@pre_opp_name}'/>
        </form>
        <button id='search_form_submit'/>
        
        <table class='list view'>
            <browser assert='{@pre_opp_name}'/>
            <link text='{@pre_opp_name}'/>
        </table>
        
        <table id='detailpanel_1'>
            <browser assert='{@pre_opp_name}'/>
        </table>
        
        <div id='list_subpanel_activities'>
            <table class='list view'>
                <button value='  Schedule Meeting  '/>
            </table>
        </div>

        <form id='form_SubpanelQuickCreate_Meetings'>
            <button id='Meetings_subpanel_full_form_button'/> 
        </form>
         
        <form id='EditView'>
            <textfield id='name' set='{@Opportunity_2281.meeting_name} {@user.name} {@stamp}'/>
            <textfield id='location' set='{@Opportunity_2281.meeting_location}'/>
            <textfield id='description' set='{@Opportunity_2281.meeting_description}'/>
            <select id='status' set='{@Opportunity_2281.meeting_status}'/>
            
            <select id='date_start_hours' set='{@Opportunity_2281.meeting_date_start_hours}'/>
            <select id='date_start_minutes' set='{@Opportunity_2281.meeting_date_start_minutes}'/>
            <!--select id='date_start_meridiem' set='{@Opportunity_2281.meeting_date_start_meridiem}'/-->
            
            <textfield name='duration_hours' set='{@Opportunity_2281.meeting_duration_hours}'/>
            <select name='duration_minutes' set='{@Opportunity_2281.meeting_duration_minutes}'/>
            
            <checkbox name='reminder_checked' set='{@Opportunity_2281.meeting_reminder_checked}'/>
        </form>
        <!--teams select-->
        <table id='EditView_team_name_table'>
        <button value='Select'/>
        </table>
        <browser assert='Team Search'/>
        <textfield name='name' set='{@Opportunity_2281.team_name}'/> 
        <button value='Search'/>
        <link text='{@Opportunity_2281.team_name}'/>
        <form id='EditView'>      
                <button value='Cancel' alert="true"/>
        </form>
         
       <div id='list_subpanel_activities'>
             <table class='list view'>              
             <span class='pageNumbers' assert='(0 - 0 of 0)'/>
             <link text='{@Opportunity_2281.meeting_name} {@user.name} {@stamp}' exist='false'/> 
           </table>
       </div>

     </csv>
     <!--Run Delete XML-->
     <puts text='---Opportunity_2281.xml---Finished!'/>
</soda>
