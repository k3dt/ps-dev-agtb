<soda>
        <csv file='{@global.scriptsdir}/csvs/opportunity/Opportunity_2286.csv' var='Opportunity_2286'>
        <puts text='----Opportunity_2286.xml--------Start!'/>
        <link id='moduleTab_Opportunities'/>
        <browser assert='Opportunities'/>
        <button id='search_form_clear'/>
        <form id='search_form'>
            <textfield id='name_basic' set='{@pre_opp_name}'/>
        </form>
        <button id='search_form_submit'/>
        
        <table class='list view'>
            <browser assert='{@pre_opp_name}'/>
            <link text='{@pre_opp_name}'/>
        </table>
        
        <table id='detailpanel_1'>
            <browser assert='{@pre_opp_name}'/>
        </table>
        
         <div id='list_subpanel_activities'>
            <table class='list view'>
               <button value='  Log Call  '/>
            </table>
         </div>         
         <button id='Calls_subpanel_full_form_button'/>
         <form id='EditView'>
            <textfield id='name' set='{@Opportunity_2286.call_name} {@user.name} {@stamp}'/>
            <textfield id='date_start_date' set='{@Opportunity_2286.call_date_start_date}'/>
            <select id='date_start_hours' set='{@Opportunity_2286.call_date_start_hours}'/>
            <select id='date_start_minutes' set='{@Opportunity_2286.call_date_start_minutes}'/>
            
            <textfield name='duration_hours' set='{@Opportunity_2286.call_duration_hours}'/>
            <select name='duration_minutes' set='{@Opportunity_2286.call_duration_minutes}'/>
            
            <select id='direction' set='{@Opportunity_2286.call_direction}'/>
            <select id='status' set='{@Opportunity_2286.call_status}'/>
            
            <checkbox name='reminder_checked' set='{@Opportunity_2286.call_reminder_checked}'/>
            <textfield id='description' set='{@Opportunity_2286.call_description}'/>             
            <!--teams select-->
            <table id='EditView_team_name_table'>
                <button value='Select'/>
            </table>
        </form>
        <browser assert='Team Search'/>
        <textfield name='name' set='{@Opportunity_2286.team_name}'/> 
        <button value='Search'/>
        <link text='{@Opportunity_2286.team_name}'/>
        <form id='EditView'>
            <button value='Cancel' alert="true"/>
        </form>
        <div id='list_subpanel_activities'>
            <table class='list view'>             
                <link text='{@Opportunity_2286.call_name} {@user.name} {@stamp}' click='false' exist='false'/>
            </table>
        </div>
    </csv>
    <!--Run Delete XML-->
    <puts text='---Opportunity_2286.xml---Finished!'/>
</soda>
