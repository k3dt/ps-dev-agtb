<soda>
	<!--
		Actions:
			a)    Log in as a valid user
			b)    Go to a Contract detail view
			c)    Click “Select” button form Products subpanel
			d)    Select a product record from pop up window
		Results:
 			a) Select product show in produce subpanel list view
	-->
	<puts text="Test: Products_10259.xml starting." />

	<!-- Create a Product and Contract -->
	<script file="{@global.scriptsdir}/modules/lib/Product_Create_Lib.xml" />

	<script file="{@global.scriptsdir}/modules/lib/Contract_Create_Lib.xml" />

	<!-- select in subpanel -->
	<button id="contracts_products_select_button" />
	<link text="{@pre_product_name}" />
	
	<browser assert="{@pre_product_name}" />

	<script file="{@global.scriptsdir}/modules/lib/Contract_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Product_Delete_Lib.xml" />
	
	<puts text="Test: Products_10259.xml finished." />
</soda>
