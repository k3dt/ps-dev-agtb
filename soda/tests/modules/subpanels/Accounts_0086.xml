<soda>
	<puts text="Test: Accounts_0086.xml starting." />

	<!--Create a account record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.xml" />

	<!--Search the record-->
	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>
	<textfield id="name_basic" set="{@pre_account_name}" />
	<button id="search_form_submit" />
	<table class="list view">
		<link text="{@pre_account_name}" />
	</table>

	<!--Create bug Organization-->
	<browser assert="{@pre_account_name}" />
	<button name="accounts_bugs_create_button" />
	<csv file="{@global.scriptsdir}/csvs/accounts/Accounts_0086.csv" var="bug_record_0086">
		<form id="form_SubpanelQuickCreate_Bugs">
			<textfield id="name" set="{@bug_record_0086.name} {@user.name} {@stamp}" />
			<textfield id="description" set="{@bug_record_0086.description}" />
			<select id="priority" set="{@bug_record_0086.priority}" />
			<select id="source" set="{@bug_record_0086.source}" />
			<select id="type" set="{@bug_record_0086.type}" />
			<select id="product_category" set="{@bug_record_0086.product_category}" />
		</form>

		<!--user select-->
		<button id="btn_assigned_user_name" />
		<browser assert="User Search" />
		<textfield name="last_name_advanced" set="{@bug_record_0086.assigned_user_name}" />
		<button value="Search" />
		<link text="{@bug_record_0086.assigned_user_name}" />

		<!--teams select-->
		<table id="form_SubpanelQuickCreate_Bugs_team_name_table">
			<button value="Select" />
		</table>
		<browser assert="Team Search" />
		<textfield name="name" set="{@bug_record_0086.team_name}" />
		<button value="Search" />
		<link text="{@bug_record_0086.team_name}" />

		<!--click "Cancel"-->
		<button id="Bugs_subpanel_cancel_button" alert="true" />

		<div id="list_subpanel_bugs">
			<span class="pageNumbers" assert="(0 - 0 of 0)" />
		</div>
	</csv>

	<!--Delete a account record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<puts text="Test: Accounts_0086.xml finished." />
</soda>
                        
