<soda>
	<!--
		Account Detail - Products subpanel - Edit: editing product related to the
		account is canceled.
	-->
	<puts text="Test: Accounts_0265.xml starting." />

	<!--Create an account record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Create_Lib.xml" />

	<!--Search the record-->
	<link id="moduleTab_Accounts" />
	<div class="moduleTitle">
		<link href="index.php?module=Accounts&amp;action=index" click="false" />
	</div>
	<wait timeout="3" />
	<link text="Basic Search" required="false" />
	<form id="search_form">
		<button id="search_form_clear" />
		<textfield id="name_basic" set="{@pre_account_name}" />
		<button id="search_form_submit" />
	</form>

	<table class="list view">
		<browser assert="{@pre_account_name}" />
		<link text="{@pre_account_name}" />
	</table>

	<!--Go to its detail view-->
	<browser assert="{@pre_account_name}" />

	<!--Create a product in Products Subpanel-->
	<div id="list_subpanel_products">
		<button name="Products_create_button" />
	</div>
	<browser assert="Create" />
	<link text="Products" click="false" />
	<csv file="{@global.scriptsdir}/csvs/accounts/Accounts_0265.csv" var="product_record_0265">
		<textfield id="name" set="{@product_record_0265.name} {@user.name} {@stamp}" />
		<textfield id="quantity" set="{@product_record_0265.quantity}" />
		<textfield id="serial_number" set="{@product_record_0265.serial_number}" />
		<textfield id="asset_number" set="{@product_record_0265.asset_number}" />
		<textfield id="date_purchased" set="{@product_record_0265.date_purchased}" />
		<textfield id="date_support_starts" set="{@product_record_0265.date_support_starts}" />
		<textfield id="date_support_expires" set="{@product_record_0265.date_support_expires}" />
		<textfield id="cost_price" set="{@product_record_0265.cost_price}" />
		<textfield id="list_price" set="{@product_record_0265.list_price}" />
		<textfield id="discount_price" set="{@product_record_0265.discount_price}" />
		<textfield id="discount_amount" set="{@product_record_0265.discount_amount}" />
		<textfield id="book_value" set="{@product_record_0265.book_value}" />
		<textfield id="book_value_date" set="{@product_record_0265.book_value_date}" />
		<textfield id="website" set="{@product_record_0265.website}" />
		<textfield id="mft_part_num" set="{@product_record_0265.mft_part_num}" />
		<textfield id="vendor_part_num" set="{@product_record_0265.vendor_part_num}" />
		<textfield id="description" set="{@product_record_0265.description}" />
		<textfield id="support_name" set="{@product_record_0265.support_name}" />
		<textfield id="support_description" set="{@product_record_0265.support_description}" />
		<textfield id="weight" set="{@product_record_0265.weight}" />
		<select id="status" set="{@product_record_0265.status}" />
		<select name="currency_id" set="{@product_record_0265.currency_id}" />
		<checkbox id="discount_select" set="{@product_record_0265.discount_select}" />
		<select id="category_id" set="{@product_record_0265.category_id}" />
		<select id="tax_class" set="{@product_record_0265.tax_class}" />
		<button value="Save" />

		<!--back to product subpanel-->
		<div id="list_subpanel_products">
			<span class="pageNumbers" assert="(1 - 1 of 1)" />
			<browser assert="{@product_record_0265.name} {@user.name} {@stamp}" />
			<!-- click the eidt link -->
			<link text="edit" />
		</div>

		<textfield id="name"
			set="{@product_record_0265.name_edit} {@user.name} {@stamp}" />
		<textfield id="quantity" set="{@product_record_0265.quantity_edit}" />
		<textfield id="serial_number" set="{@product_record_0265.serial_number_edit}" />
		<textfield id="asset_number" set="{@product_record_0265.asset_number_edit}" />
		<textfield id="date_purchased" set="{@product_record_0265.date_purchased_edit}" />
		<textfield id="date_support_starts" set="{@product_record_0265.date_support_starts_edit}" />
		<textfield id="date_support_expires" set="{@product_record_0265.date_support_expires_edit}" />
		<textfield id="cost_price" set="{@product_record_0265.cost_price_edit}" />
		<textfield id="list_price" set="{@product_record_0265.list_price_edit}" />
		<textfield id="discount_price" set="{@product_record_0265.discount_price_edit}" />
		<textfield id="discount_amount" set="{@product_record_0265.discount_amount_edit}" />
		<textfield id="book_value" set="{@product_record_0265.book_value_edit}" />
		<textfield id="book_value_date" set="{@product_record_0265.book_value_date_edit}" />
		<textfield id="website" set="{@product_record_0265.website_edit}" />
		<textfield id="mft_part_num" set="{@product_record_0265.mft_part_num_edit}" />
		<textfield id="vendor_part_num" set="{@product_record_0265.vendor_part_num_edit}" />
		<textfield id="description" set="{@product_record_0265.description_edit}" />
		<textfield id="support_name" set="{@product_record_0265.support_name_edit}" />
		<textfield id="support_description" set="{@product_record_0265.support_description_edit}" />
		<textfield id="weight" set="{@product_record_0265.weight_edit}" />
		<select id="status" set="{@product_record_0265.status_edit}" />
		<select name="currency_id" set="{@product_record_0265.currency_id_edit}" />
		<checkbox id="discount_select" set="{@product_record_0265.discount_select_edit}" />
		<select id="category_id" set="{@product_record_0265.category_id_edit}" />
		<select id="tax_class" set="{@product_record_0265.tax_class_edit}" />
		<button value="Cancel" alert="true" />

		<div id="list_subpanel_products">
			<browser assert="{@product_record_0265.name} {@user.name} {@stamp}" />
			<link text="{@product_record_0265.name} {@user.name} {@stamp}" />
		</div>

		<div id="Products_detailview_tabs">
			<browser assert="{@product_record_0265.name} {@user.name} {@stamp}" />
			<browser assert="{@product_record_0265.quantity}" />
			<browser assert="{@product_record_0265.serial_number}" />
			<browser assert="{@product_record_0265.asset_number}" />
			<browser assert="{@product_record_0265.date_purchased}" />
			<browser assert="{@product_record_0265.date_support_starts}" />
			<browser assert="{@product_record_0265.date_support_expires}" />
			<browser assert="{@product_record_0265.cost_verify}" />
			<browser assert="{@product_record_0265.list_verify}" />
			<browser assert="{@product_record_0265.discount_verify}" />
			<browser assert="{@product_record_0265.discount_rate_verify}" />
			<browser assert="{@product_record_0265.book_value_verify}" />
			<browser assert="{@product_record_0265.book_value_date}" />
			<browser assert="{@product_record_0265.website}" />
			<browser assert="{@product_record_0265.mft_part_num}" />
			<browser assert="{@product_record_0265.vendor_part_num}" />
			<browser assert="{@product_record_0265.description}" />
			<browser assert="{@product_record_0265.support_name}" />
			<browser assert="{@product_record_0265.support_description}" />
			<browser assert="{@product_record_0265.weight_verify}" />
			<browser assert="{@product_record_0265.status}" />
		</div>

	</csv>

	<!--Delete a account record-->
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Product_Delete_Lib.xml" />
	<puts text="Test: Accounts_0265.xml finished." />
</soda>
