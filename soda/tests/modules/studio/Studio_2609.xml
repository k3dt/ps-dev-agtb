<soda>

	<!--
		Create Custom Filed_Verify that "Drop Down" type custom field can be created.
	-->
	<puts text="Test: Studio_2609.xml starting" />

	<!-- Create Custom Field in Studio -->
	<link text="Admin" />
	<link text="Studio" />

	<div id="mbfooter">
		<button value="Dropdown Editor" />
	</div>
	<wait timeout="5" />

	<div id="mbtabs">
		<button name="adddropdownbtn" />
	</div>
	<wait timeout="3" />
	<timestamp />

	<textfield id="dropdown_name" set="testDropD_{@stamp}" />
	<wait timeout="3" />
	<textfield id="drop_name" set="drop_name" />
	<textfield id="drop_value" set="drop_value" />
	<button id="dropdownaddbtn" />
	<wait timeout="3" />
	<button id="saveBtn" jscriptevent="onclick" />


	<wait timeout="3" />

	<link text="testDropD_{@stamp}" />

	<puts text="Test: Studio_2609.xml finished." />
</soda>