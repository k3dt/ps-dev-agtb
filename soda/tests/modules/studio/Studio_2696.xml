<soda>
	<!--
		Edit Customized fields in related modules_Verified that corresponding page is
		displayed when clicking a link right after saving the newly created drop down.
	-->
	<puts text="Test: Studio_2696.xml starting" />

	<!-- Create Custom Field in Studio -->
	<link text="Admin" />
	<link text="Studio" assertPage="false" />
	<link text="Accounts" class="studiolink" assertPage="false" />
	<link text="Fields" class="studiolink" assertPage="false" />
	<button name="addfieldbtn" />

	<select id="type" set="DropDown" />
	<wait timeout="3" />
	<textfield id="field_name_id" set="testdropdown" />
	<wait timeout="3" />

	<form id="popup_form_id">
		<button value="Add" />
	</form>

	<textfield id="dropdown_name" set="dropDownName2" />
	<textfield id="drop_name" set="dropName_name" />
	<textfield id="drop_value" set="dropValue_value" />
	<button id="dropdownaddbtn" />

	<button id="saveBtn" />
	<wait timeout="6" />
	<button name="fsavebtn" />
	<wait timeout="6" />

	<!-- Verify creation of Date type field -->
	<link text="testdropdown_c" click="false" />

	<!-- Verify that clicking another link works -->
	<link text="ownership" />
	<form id="popup_form_id">
		<textfield id="label_value_id" assert="Ownership:" />
	</form>
	<button name="cancelbtn" />

	<!-- Delete field after verification -->
	<link text="testdropdown_c" />
	<button name="fdeletebtn" alert="true" />

	<puts text="Test: Studio_2696.xml finished." />
</soda>