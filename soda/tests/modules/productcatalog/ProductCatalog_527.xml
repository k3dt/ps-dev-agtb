<soda>
    <csv file="{@global.scriptsdir}/csvs/productcatalog/ProductCatalog_527.csv" var="catalog_record_527" >
        <puts text="---ProductCatalog_527.xml---Start!" />
        <!--Create_Product_catalog.-->
		<script file='{@global.scriptsdir}/modules/lib/ProductCategory_Create_Lib.xml' />
		<script file='{@global.scriptsdir}/modules/lib/Manufacturer_Create_Lib.xml' />
		<script file='{@global.scriptsdir}/modules/lib/ProductType_Create_Lib.xml' /> 
        <link href="index.php?module=Administration&amp;action=index" />
	    <link text="Product Catalog" />
        <timestamp/>
        <link text="Create Product for Catalog" />
        <form id="EditView" >
            <textfield name="name" set="{@catalog_record_527.name} {@user.name} {@stamp}" />
            <button id="btn_category_name" />
        </form>
        <table class="list view" >
            <link text="{@ProductCategory_Create_Lib}" />
        </table>
        <form id="EditView" >
            <textfield id="website" set="{@catalog_record_527.website}" />
            <select id="tax_class" set="{@catalog_record_527.tax_class}" />
            <select id="manufacturer_id" set="{@Manufacturer_Create_Lib}" />
            <textfield id="mft_part_num" set="{@catalog_record_527.mft_part_num}" />

            <textfield id="vendor_part_num" set="{@catalog_record_527.vendor_part_num}" />
            <textfield id="cost_price" set="{@catalog_record_527.cost_price}" />
            <textfield id="list_price" set="{@catalog_record_527.list_price}" />
            <textfield id="discount_price" set="{@catalog_record_527.discount_price}" />

            <select id="pricing_formula" set="{@catalog_record_527.Default_Pricing_Formula}" />
            <textfield id="pricing_factor_ProfitMargin" set="{@catalog_record_527.Pricing_Factor}" />
            <textfield id="description" set="{@catalog_record_527.description}" />
            <select id="status" set="{@catalog_record_527.status}" />
            <textfield id="date_available" set="{@catalog_record_527.date_available}" />
            <textfield id="qty_in_stock" set="{@catalog_record_527.qty_in_stock}" />
            <textfield id="weight" set="{@catalog_record_527.weight}" />

            <select id="type_id" set="{@ProductType_Create_Lib}" />
            <textfield id="support_name" set="{@catalog_record_527.support_name}" />
            <textfield id="support_contact" set="{@catalog_record_527.support_contact}" />
            <textfield id="support_description" set="{@catalog_record_527.support_description}" />
            <select id="support_term" set="{@catalog_record_527.support_term}" />

            <button name="button" value="Save" />
        </form>

        <table id="detailpanel_1" >
            <browser assert="{@catalog_record_527.name} {@user.name} {@stamp}" />
        </table>
        
		<link href="index.php?module=ProductTemplates&amp;action=index&amp;return_module=ProductTemplates&amp;return_action=DetailView" />
        <link text="Basic Search" required="false" />
        <button id="search_form_clear" />
        <form id="search_form" >
            <textfield id="name_basic" set="{@catalog_record_527.name} {@user.name} {@stamp}" />
        </form>
        <button id="search_form_submit" />

        <table class="list view" >
            <browser assert="{@catalog_record_527.name} {@user.name} {@stamp}" />
        </table>

    </csv>
    <!--Run Delete XML-->
    <script file='{@global.scriptsdir}/modules/lib/ProductCatalog_Delete_Lib.xml' />
    <script file='{@global.scriptsdir}/modules/lib/ProductCategory_Delete_Lib.xml' />
    <script file='{@global.scriptsdir}/modules/lib/Manufacturer_Delete_Lib.xml' />
    <script file='{@global.scriptsdir}/modules/lib/ProductType_Delete_Lib.xml' />
    <puts text="---ProductCatalog_527.xml---Finished!" />
</soda>

