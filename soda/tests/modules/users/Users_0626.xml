<soda>
    	   <puts text="---User_0626:verify records created in 'global' team can be accessed by any user---Start!"/>
           <csv file="{@global.scriptsdir}/csvs/users/Users_0626.csv" var="user_record_0626">
               <!--Create record-->
               <script file='{@global.scriptsdir}/modules/lib/{@user_record_0626.module}'/>
           </csv>
           <button id='edit_button'/>
           <!--teams select-->
           <table id='EditView_team_name_table'>
                 <image id='remove_team_name_collection_0' click='true'/>
                 <button value='Select'/>
            </table>    
            <browser assert='Team Search'/>
            <textfield name='name' set='Global'/> 
            <button value='Search'/>
            <link text='Global'/>
            <radio id='primary_team_name_collection_0' set='true'/>
            <button value='Save'/>
            <link id='{@user_record_0626.modulelink}'/>
            <browser assert='{@user_record_0626.modulecheck}'/>
            <button value='Clear'/>            
            <textfield id='name_basic' set='{@pre_account_name}'/>
            <button id='search_form_submit'/>            
            <table class='list view'>
               <span class='pageNumbers' assert='(1 - 1 of 1)'/>
            </table> 
           <!--Login in as normal user-->
           <link text="Log Out"/>
           <csv file='{@global.scriptsdir}/modules/lib/SugarInit_Lib.csv' var='SugarInit0626'>
                <textfield id='user_name' set='{@SugarInit0626.last_name}' />
                <textfield id='user_password' set='{@SugarInit0626.pass}'/>
                <button id='login_button'/>
                <button value='  Next  ' required='false' timeout='3'/>
                <button value='  Next  ' required='false' timeout='3'/>
                <button value='  Next  ' required='false' timeout='3'/>
                <button value='  Finish  ' required='false' timeout='3'/>
            </csv>
            <!--change page style 'modules'-->
            
            <link id='{@user_record_0626.modulelink}'/>
            <button value='Clear'/>
            <browser assert='{@user_record_0626.modulecheck}'/>
            <textfield id='name_basic' set='{@pre_account_name}'/>
            <button id='search_form_submit'/>      
            <table class='list view'>
                <span class='pageNumbers' assert='(1 - 1 of 1)'/>
            </table> 
            <!--Login back to Admin-->
            <link text="Log Out"/>
            <script file='{@global.scriptsdir}/modules/lib/login.xml'/>
            <!--delete records-->
            <script file='{@global.scriptsdir}/modules/lib/{@user_record_0626.deletemodule}'/>
            <puts text="---User_0626:records created in 'global' team can be accessed by any user---Finished!"/>
</soda>
