<soda>
    <puts text='---User_9647:verify no popup when create inactive group user ---Start!'/>
    <timestamp/>
    <!--goto Admin-->
    <div id='globalLinks'>
        <link text='Admin'/>
    </div>
    <!--click 'User Management'-->
    <link href='./index.php?module=Users&amp;action=index'/>
    <browser assert='Users'/>
    <!--Create a new normal user-->
    <link text='Create Group User'/>
    <browser assert='Users'/>
    <csv file='{@global.scriptsdir}/csvs/users/Users_9647.csv' var='Users9647'>
          <textfield name='sugar_user_name' set='{@Users9647.last_name}{@stamp}' jscriptevent="onkeyup" />
          <textfield id='unique_name' set='{@Users9647.last_name}{@stamp}' jscriptevent="onkeyup"/>
          <select name='status' set='Inactive'/>
          <button id='Save'/>
          <!--search user-->
          <link text='Users'/>
          <link text='Advanced Search' required='false' timeout='3'/>
          <form name='search_form'>
              <button value='Clear'/>
              <textfield name='user_name_advanced' set='{@Users9647.last_name}{@stamp}'/>
              <select name='status_advanced[]' set='Inactive'/>
              <button value='Search'/>
           </form>
           <table class='list view'>
               <span class='pageNumbers' assert='(1 - 1 of 1)'/>
           </table>
           <!--Clear search-->
           <link text='Basic Search'/>
           <form name='search_form'>
                <button value='Clear'/>
           </form>
       </csv>
     <puts text='---User_9647:verify no popup when create inactive group user---Finished!'/>
</soda>
