<soda>
    <puts text='---User_8624:Popup picker user---Start!'/>
    <link id='moduleTab_Accounts'/>
    <browser assert='Accounts'/>
    <link text='Create Account'/>
    <browser assert='Create'/>
    <!--user select-->
    <button id='btn_assigned_user_name'/>
    <browser assert='User Search'/> 
        <form id='popup_query_form'>
            <browser assert='First Name'/>
            <browser assert='Last Name'/>
            <browser assert='User Name'/>
            <browser assert='Group User'/>
        </form>
        <link text='{@user.name}'/>
        <form id='EditView'>
            <button value='Cancel'/>
        </form>
        <browser assert='Accounts'/>
        
        <table class='list view'>
                <span class='pageNumbers' assert='(0 - 0 of 0)'/>
        </table>
     <puts text='---User_8624:verify popup picker user---Finished!'/>
</soda>
