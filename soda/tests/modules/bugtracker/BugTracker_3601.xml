<soda>
	<!-- List View_Verify that all bugs are displayed without any error after clicking "Cancel" button on "MERGE RECORDS WITH:.." page. -->
	<puts text="Test: Bug Tracker_3601.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/bugtracker/BugTrackerAssertText.csv" var="bugtracker" />
	<timestamp />

	<!--Create three bug tracker records-->
	<script file="{@global.scriptsdir}/modules/lib/Bug_Create_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Bug_Create_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Bug_Create_Lib.xml" />

	<!--search these three records-->
	<link id="moduleTab_Bugs" />
	<div class="moduleTitle">
		<link href="index.php?module=Bugs&amp;action=index" click="false" />
	</div>
	<textfield id="name_basic" set="{@bug_record.name}" />
	<button id="search_form_submit" />
	<table class="list view">
		<link text="{@pre_bug_name}" click="false" />
		<span class="pageNumbers" assert="(1 - 3 of 3)" />
	</table>

	<!--click merge button-->
	<checkbox name="massall" set="true" />
	<link text="Actions" />
	<link text="Merge" />
	<div class="moduleTitle">
		<link href="index.php?module=Bugs&amp;action=index" click="false" />
		<browser assert="{@bugtracker.merge_confirmation}" />
	</div>

	<!--cancel merge-->
	<button value="  Cancel  " />
	<div class="moduleTitle" assert="{@pre_bug_name}" click="false" />

	<!--verify the three records are still there-->
	<textfield id="name_basic" set="{@bug_record.name}" />
	<button id="search_form_submit" />
	<table class="list view">
		<link text="{@pre_bug_name}" click="false" />
		<span class="pageNumbers" assert="(1 - 3 of 3)" />
	</table>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Bug_Delete_Lib.xml" />

	<puts text="Test: Bug Tracker_3601.xml finished." />
</soda>