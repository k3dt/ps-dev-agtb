<soda>
    <puts text="---Teammanagement_10384:Delete private team from detail view with the corresponding user still exists---Start!" />
    <!--goto Admin-->
    <div id="globalLinks">
        <link text="Admin" />
    </div>
	<csv file="{@global.scriptsdir}/modules/lib/SugarInit_Lib.csv" var="SugarInit">
		<!--click "Team Management"-->
		<link href="./index.php?module=Teams&amp;action=index" />
		<browser assert="Teams" />
		<!--Search team-->
		<button value="Clear" />
		<textfield name="name_basic" set="{@SugarInit.last_name}" />
		<button value="Search" />
		<table class="list view">
			<link text="{@SugarInit.last_name}" click="true" />
			<wait timeout="5" />
		</table>
		<!--try to delete team from detail view-->
		<browser assert="Teams: {@SugarInit.last_name}" />
		<button value=" Delete " alert="true" />
		<browser assert="This private team cannot be deleted unless the user [{@SugarInit.last_name}] is deleted." />
		
		<!--verify team not delelted-->
		<link text="Teams" />
		<browser assert="Teams" />
		
		<button value="Clear" />
		<textfield name="name_basic" set="{@SugarInit.last_name}" />
		<button value="Search" />
		<table class="list view">
			<link text="{@SugarInit.last_name}" click="false" />
		</table>
	</csv>

    <puts text="---Teammanagement_10384---Finished!" />
</soda>
