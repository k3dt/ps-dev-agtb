<soda>
	<puts text="Test: Opportunity_2339.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/opportunity/Opportunity_2339.csv" var="Opportunity_2339">

		<!--script file="scripts/sugarcrm/modules/lib/Team_Create_Lib.xml"/ -->
		<script file="{@global.scriptsdir}/modules/lib/Opp_Create_Lib.xml" />
		<link id="moduleTab_Opportunities" />

		<link text="Basic Search" required="false" />

		<button id="search_form_clear" />

		<form id="search_form">
			<textfield id="name_basic" set="{@Opportunity_2339.name} {@user.name} {@stamp}" />
		</form>

		<button id="search_form_submit" class="button" type="submit" value="Search"
			name="button" onclick="SUGAR.savedViews.setChooser()" />

		<table class="list view">
			<browser assert="{@Opportunity_2339.name} {@user.name} {@stamp}" />
			<link text="{@Opportunity_2339.name} {@user.name} {@stamp}" click="true" />
		</table>
		<table id="detailpanel_1">

			<browser assert="{@Opportunity_2339.name} {@user.name} {@stamp}" />
		</table>
		<timestamp />

		<button id="opportunity_leads_create_button" />

		<wait timeout="10" />
		<!--teams select -->
		<button name="teamSelect" />

		<wait timeout="10" />

		<browser assert="Team Search" />
		<textfield name="name" set="{@Opportunity_2339.team_name}" />
		<button value="Search" />
		<link text="{@Opportunity_2339.team_name}" />

		<select id="lead_source" set="{@Opportunity_2339.lead_lead_source}" />
		<select id="status" set="{@Opportunity_2339.lead_status}" />
		<!--select id="salutation" set="{@Opportunity_2339.salutation}"/> <textfield id="lead_source_description" 
			set="{@Opportunity_2339.lead_source_description}"/> <textfield id="status_description" 
			set="{@Opportunity_2339.status_description}"/> <textfield id="opportunity_amount" 
			set="{@Opportunity_2339.opportunity_amount}"/ -->
		<textfield id="refered_by" set="{@Opportunity_2339.lead_refered_by}" />

		<textfield name="account_name" set="{@Opportunity_2339.lead_account_name}" />
		<textfield name="first_name" set="{@Opportunity_2339.lead_first_name}" />
		<textfield id="last_name" set="{@Opportunity_2339.lead_last_name}" />
		<textfield id="title" set="{@Opportunity_2339.lead_title}" />
		<textfield id="phone_work" set="{@Opportunity_2339.lead_phone_work}" />
		<textfield id="phone_mobile" set="{@Opportunity_2339.lead_phone_mobile}" />
		<textfield id="phone_fax" set="{@Opportunity_2339.lead_phone_fax}" />
		<!--textfield id="phone_home" set="{@Opportunity_2339.phone_home}"/> <textfield 
			id="phone_other" set="{@Opportunity_2339.phone_other}"/ -->
		<textfield id="department" set="{@Opportunity_2339.lead_department}" />
		<textfield id="Leads0emailAddress0" set="{@Opportunity_2339.lead_emailAddress0}" />
		<checkbox id="do_not_call" set="{@Opportunity_2339.lead_do_not_call}" />
		<!--textfield id="description" set="{@Opportunity_2339.description}"/ -->
		<button id="Leads_subpanel_cancel_button" alert="true" />
		<div id="list_subpanel_leads">
			<table class="list view">
				<browser
					assertnot="{@Opportunity_2339.lead_first_name} {@Opportunity_2339.lead_last_name}" />
			</table>
		</div>
		<hidden name="record" var="pre_opp_id" />

		<var var="pre_opp_name" set="{@Opportunity_2339.name} {@user.name} {@stamp}" />

	</csv>
	<!--Run Delete XML -->
	<script file="{@global.scriptsdir}/modules/lib/Opp_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Account_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Lead_Delete_Lib.xml" />
	<!--script file="scripts/sugarcrm/modules/lib/Team_Delete_Lib.xml"/ -->
	<puts text="Test: Opportunity_2339.xml finished." />
</soda>
