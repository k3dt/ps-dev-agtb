<soda>
	<!--
		Action:
			1. Login to system as a valid user.
			2. Click "Calendar" navigation tab.
			3. Click "Meetings" link on Navigation shortcuts.
			2,3. mfleeger - meetings are a tab now.
			4. Click any meeting subject link in "Meeting List" view.
			5. Click "Edit" button on "Meeting Detail View" page.
			6. Update meeting information on "Edit Meeting" page.
			7. Click "Cancel" button on "Edit Meeting" page.
		Expectation:
			7. Meeting information is not updated.
	-->
	<puts text="Test: Calendar_0700.xml starting." />
	
	<script file="{@global.scriptsdir}/modules/lib/Meeting_Create_Lib.xml" />
	
	<link id="moduleTab_Meetings" />
	
	<textfield id="name_basic" set="{@pre_meeting_name}" />
	<button id="search_form_submit" />
	
	<browser assert="{@pre_meeting_name}" />
	<link text="{@pre_meeting_name}" />
	
	<wait timeout="3" />
	
	<button id="edit_button" />
	
	<textfield id="name" set="Modified_{@pre_meeting_name}" />
	
	<button value="Cancel" alert="true" />
	
	<table id="detailpanel_1">
		<browser assert="{@pre_meeting_name}" />
		<browser assertnot="Modified_{@pre_meeting_name}" />
	</table>
	
	<script file="{@global.scriptsdir}/modules/lib/Meeting_Delete_Lib.xml" />
	
	<puts text="Test: Calendar_0700.xml finished." />
</soda>