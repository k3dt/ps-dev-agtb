<soda>
	<puts text="Test: Reports_8339.xml starting." />
	<!--
	Action:
		1. Login with a valid user.
		2. Go to "Reports" module.
		3. Click Report "New Cases By Month" in the all reports list view.
		4. Click "Edit" button.

	Expected Results:
		4. Report page is 'define filter' page at once
	-->
	
	<link id="moduleTab_Reports" />

	<var var="report_name" set="New Cases By Month" />
	
	<textfield name="name_basic" set="{@report_name}" />
	<button id="search_form_submit" />
	
	<browser assert="New Cases By Month" />
	<link text="{@report_name}" />
	
	<button id="editReportButton" />
	<wait timeout="4" />

	<button id="add_btn_Filter.1" click="false" />
	
	<puts text="Test: Reports_8339.xml finished." />
</soda>	