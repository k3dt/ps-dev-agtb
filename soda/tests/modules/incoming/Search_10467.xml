<soda>
    	   <puts text="---Search_10467 Search out open items in Tasks module---Start!" />
		   <!--create tasks-->
		    <script file="{@global.scriptsdir}/modules/lib/Task_Create_Lib.xml" />
			<button id="edit_button" />
			<textfield id="name" set="Not Started" />
			<select id="status" set="Not Started" />
			<button value="Save" />
			<div id ="LBL_TASK_INFORMATION">
				<browser assert="Not Started" />
			</div>

			<script file="{@global.scriptsdir}/modules/lib/Task_Create_Lib.xml" />
			<button id="edit_button" />
			<textfield id="name" set="In Progress" />
			<select id="status" set="In Progress" />
			<button value="Save" />
			<div id ="LBL_TASK_INFORMATION">
				<browser assert="In Progress" />
			</div>

			<script file="{@global.scriptsdir}/modules/lib/Task_Create_Lib.xml" />
			<button id="edit_button" />
			<textfield id="name" set="Completed" />
			<select id="status" set="Completed" />
			<button value="Save" />
			<div id ="LBL_TASK_INFORMATION">
				<browser assert="Completed" />
			</div>

			<script file="{@global.scriptsdir}/modules/lib/Task_Create_Lib.xml" />
			<button id="edit_button" />
			<textfield id="name" set="Pending Input" />
			<select id="status" set="Pending Input" />
			<button value="Save" />
			<div id ="LBL_TASK_INFORMATION">
				<browser assert="Pending Input" />
			</div>

			<script file="{@global.scriptsdir}/modules/lib/Task_Create_Lib.xml" />
			<button id="edit_button" />
			<textfield id="name" set="Deferred" />
			<select id="status" set="Deferred" />
			<button value="Save" />
			<div id ="LBL_TASK_INFORMATION">
				<browser assert="Deferred" />
			</div>

			
			<link id='moduleTab_Tasks'/>
			<link text="Basic Search" required="false" timeout="3" />
			<button id="search_form_clear" />
			<button id="search_form_submit" />
			<table class="list view">
					<span class='pageNumbers' assert='(1 - 5 of 5)' />
					<browser assert="Completed" />
					<browser assert="Deferred" />
			</table>
			<checkbox id="open_only_basic" set="true" />
			<button id="search_form_submit" />
			<table class="list view">
					<span class='pageNumbers' assert='(1 - 3 of 3)' />
					<browser assertnot="Completed" />
					<browser assertnot="Deferred" />					
			</table>

			<script file="{@global.scriptsdir}/modules/lib/Task_Delete_Lib.xml" />
			

            <puts text="---Search_10467---Finished!" />
            
</soda>
