<soda>
    <csv file='{@global.scriptsdir}/csvs/quote/Quote_2529.csv' var='Quote_2529'>
        <puts text='----Quote_2529.xml----Edit Task for Quote_Verify that call status related to this Quote can be modified.----Start!----'/>            
        <link id='moduleTab_Quotes'/>
        <browser assert='Quotes'/>
        <form id='search_form'>
            <button id='search_form_clear'/>
            <textfield id='name_basic' set='{@pre_Quote_name}'/>
            <button id='search_form_submit'/>
        </form>

        <table class='list view'>
            <link text='{@pre_Quote_name}'/>
        </table>
        
        <div id='list_subpanel_activities'>
            <table class='list view'>
                <button value='  Log Call  '/>
            </table>
        </div>
        <timestamp/>
        <!--teams select-->
        <table id='form_SubpanelQuickCreate_Calls_team_name_table'>
            <button value='Select'/>
        </table>
        <browser assert='Team Search'/>
        <textfield name='name' set='{@Quote_2529.team_name}'/> 
        <button value='Search'/>
        <link text='{@Quote_2529.team_name}'/>
        
            
        <form id='form_SubpanelQuickCreate_Calls'>
            <textfield id='name' set='{@Quote_2529.call_name} {@user.name} {@stamp}'/>
            <textfield id='date_start_date' set='{@Quote_2529.call_date_start_date}'/>
            <select id='date_start_hours' set='{@Quote_2529.call_date_start_hours}'/>
            <select id='date_start_minutes' set='{@Quote_2529.call_date_start_minutes}'/>

            <textfield name='duration_hours' set='{@Quote_2529.call_duration_hours}'/>
            <select name='duration_minutes' set='{@Quote_2529.call_duration_minutes}'/>

            <select id='direction' set='{@Quote_2529.call_direction}'/>
            <select id='status' set='{@Quote_2529.call_status}'/>

            <checkbox name='reminder_checked' set='{@Quote_2529.call_reminder_checked}'/>
            <textfield id='description' set='{@Quote_2529.call_description}'/>            
            <button value='Save'/>
        </form>         
        <wait timeout='5'/>
        <div id='list_subpanel_activities'>
            <table class='list view'>
                <browser assert='{@Quote_2529.call_name} {@user.name} {@stamp}'/>
                <link text='{@Quote_2529.call_name} {@user.name} {@stamp}' click='false'/>
                <link class='listViewTdToolsS1' index='1' />
            </table>
        </div>
        <form id='EditView'>
            <textfield id='name' set='{@Quote_2529.call_name_edit} {@user.name} {@stamp}'/>
            <textfield id='date_start_date' set='{@Quote_2529.call_date_start_date_edit}'/>
            <select id='date_start_hours' set='{@Quote_2529.call_date_start_hours_edit}'/>
            <select id='date_start_minutes' set='{@Quote_2529.call_date_start_minutes_edit}'/>

            <textfield name='duration_hours' set='{@Quote_2529.call_duration_hours_edit}'/>
            <select name='duration_minutes' set='{@Quote_2529.call_duration_minutes_edit}'/>

            <select id='direction' set='{@Quote_2529.call_direction_edit}'/>
            <select id='status' set='{@Quote_2529.call_status_edit}'/>

            <checkbox name='reminder_checked' set='{@Quote_2529.call_reminder_checked_edit}'/>
            <textfield id='description' set='{@Quote_2529.call_description_edit}'/>            
            <button value='Save'/>
        </form>
        <div id='list_subpanel_activities'>
            <table class='list view'>             
                <link text='{@Quote_2529.call_name_edit} {@user.name} {@stamp}' click='false'/>       
            </table>
        </div>
    </csv>
    <!--Run Delete XML-->
    <script file='{@global.scriptsdir}/modules/lib/Call_Delete_Lib.xml'/>
    <puts text='---Quote_2529.xml---Finished!'/>   
</soda>
