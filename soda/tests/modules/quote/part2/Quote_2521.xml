<soda>
    <csv file='{@global.scriptsdir}/csvs/quote/Quote_2521.csv' var='Quote_2521'>
        <puts text='----Quote_2521.xml----Full Form Create Task for Quote_Verify that the task can be created for quote in Activities sub-panel in the quote detail view.----Start!----'/>
        <link id='moduleTab_Quotes'/>
        <browser assert='Quotes'/>
        <form id='search_form'>
            <button id='search_form_clear'/>
            <textfield id='name_basic' set='{@pre_Quote_name}'/>
            <button id='search_form_submit'/>
        </form>

        <table class='list view'>
            <link text='{@pre_Quote_name}'/>
        </table>

        <div id='list_subpanel_activities'>
            <table class='list view'>
                <button value='  Create Task  '/>
            </table>
        </div>
        
        <form id='form_SubpanelQuickCreate_Tasks'>
            <button id='Tasks_subpanel_full_form_button'/>
        </form>
        <timestamp/>
        <form id='EditView'>
            <textfield id='name' set='{@Quote_2521.task_name} {@user.name} {@stamp}'/>
            <select id='priority' set='{@Quote_2521.task_priority}'/>
            <select id='status' set='{@Quote_2521.task_status}'/>
            <textfield id='description' set='{@Quote_2521.task_description}'/>

            <!--Due Date-->
            <textfield id='date_due_date' set='{@Quote_2521.task_date_due_date}'/>
            <select id='date_due_hours' set='{@Quote_2521.task_date_due_hours}'/>
            <select id='date_due_minutes' set='{@Quote_2521.task_date_due_minutes}'/>
            <!--select id='date_due_meridiem' set='{@Quote_2521.task_date_due_meridiem}'/-->
            <checkbox id='date_due_flag' set='true'/>

            <!--Start Date-->
            <textfield id='date_start_date' set='{@Quote_2521.task_date_start_date}'/>
            <select id='date_start_hours' set='{@Quote_2521.task_date_start_hours}'/>
            <select id='date_start_minutes' set='{@Quote_2521.task_date_start_minutes}'/>
            <!--select id='date_start_meridiem' set='{@Quote_2521.task_date_start_meridiem}'/-->
            <checkbox id='date_start_flag' set='true'/>
        </form>
        <!--teams select-->
        <table id='EditView_team_name_table'>
            <button value='Select'/>
        </table>
        <browser assert='Team Search'/>
        <textfield name='name' set='{@Quote_2521.team_name}'/> 
        <button value='Search'/>
        <link text='{@Quote_2521.team_name}'/>
        <form id='EditView'>
            <button value='Save'/>
        </form>

        <div id='list_subpanel_activities'>
            <table class='list view'>
                <browser assert='{@Quote_2521.task_name} {@user.name} {@stamp}'/>
            </table>
        </div>
    </csv>
    <!--Run Delete XML-->
    <script file='{@global.scriptsdir}/modules/lib/Task_Delete_Lib.xml'/>
    <puts text='---Quote_2521.xml---Finished!'/>
</soda>
