<soda>
    <csv file='{@global.scriptsdir}/csvs/quote/Quote_2528.csv' var='Quote_2528'>
        <puts text='----Quote_2528.xml----Edit Meeting for Quote_Verify that meeting status related to this Quote can be modified.----Start!----'/>            
        <link id='moduleTab_Quotes'/>
        <browser assert='Quotes'/>
        <form id='search_form'>
            <button id='search_form_clear'/>
            <textfield id='name_basic' set='{@pre_Quote_name}'/>
            <button id='search_form_submit'/>
        </form>

        <table class='list view'>
            <link text='{@pre_Quote_name}'/>
        </table>
        
        <div id='list_subpanel_activities'>
            <table class='list view'>
                <button value='  Schedule Meeting  '/>
            </table>
        </div>
        
        <form id='form_SubpanelQuickCreate_Meetings'>
            <button id='Meetings_subpanel_full_form_button'/> 
        </form>
        
        <!--teams select-->
        <table id='EditView_team_name_table'>
        <button value='Select'/>
        </table>
        <browser assert='Team Search'/>
        <textfield name='name' set='{@Quote_2528.team_name}'/> 
        <button value='Search'/>
        <link text='{@Quote_2528.team_name}'/>
         
        <form id='EditView'>
            <textfield id='name' set='{@Quote_2528.meeting_name} {@user.name} {@stamp}'/>
            <textfield id='location' set='{@Quote_2528.meeting_location}'/>
            <textfield id='description' set='{@Quote_2528.meeting_description}'/>
            <select id='status' set='{@Quote_2528.meeting_status}'/>

            <select id='date_start_hours' set='{@Quote_2528.meeting_date_start_hours}'/>
            <select id='date_start_minutes' set='{@Quote_2528.meeting_date_start_minutes}'/>
            <!--select id='date_start_meridiem' set='{@Quote_2528.meeting_date_start_meridiem}'/-->

            <textfield name='duration_hours' set='{@Quote_2528.meeting_duration_hours}'/>
            <select name='duration_minutes' set='{@Quote_2528.meeting_duration_minutes}'/>

            <checkbox name='reminder_checked' set='{@Quote_2528.meeting_reminder_checked}'/>
            <button value='Save'/>
        </form>
        
         
        <div id='list_subpanel_activities'>
            <table class='list view'>              
                <span class='pageNumbers' assert='(1 - 1 of 1)'/>
                <link text='{@Quote_2528.meeting_name} {@user.name} {@stamp}' click='true'/> 
            </table>
        </div>
        <table id='detailpanel_1'>
            <browser assert='{@Quote_2528.meeting_name} {@user.name} {@stamp}'/>
        </table>
        
        <form id='form'>
           <button value='Edit' id='edit_button'/>
        </form>
       
        <timestamp/> 
        <!--teams select-->
        <table id='EditView_team_name_table'>
        <button value='Select'/>
        </table>
        <browser assert='Team Search'/>
        <textfield name='name' set='{@Quote_2528.team_name}'/> 
        <button value='Search'/>
        <link text='{@Quote_2528.team_name}'/>

        <form id='EditView'>
        <textfield id='name' set='{@Quote_2528.meeting_name_edit} {@user.name} {@stamp}'/>
        <textfield id='location' set='{@Quote_2528.meeting_location_edit}'/>
        <textfield id='description' set='{@Quote_2528.meeting_description_edit}'/>
        <select id='status' set='{@Quote_2528.meeting_status_edit}'/>

        <select id='date_start_hours' set='{@Quote_2528.meeting_date_start_hours_edit}'/>
        <select id='date_start_minutes' set='{@Quote_2528.meeting_date_start_minutes_edit}'/>
        <!--select id='date_start_meridiem' set='{@Quote_2528.meeting_date_start_meridiem_edit}'/-->

        <textfield name='duration_hours' set='{@Quote_2528.meeting_duration_hours_edit}'/>
        <select name='duration_minutes' set='{@Quote_2528.meeting_duration_minutes_edit}'/>

        <checkbox name='reminder_checked' set='{@Quote_2528.meeting_reminder_checked_edit}'/>  
        <button value='Save'/>
        </form>

        <table id='detailpanel_1'>
            <browser assert='{@Quote_2528.meeting_name_edit} {@user.name} {@stamp}'/>
        </table>      
    </csv>
    <!--Run Delete XML-->
    <script file='{@global.scriptsdir}/modules/lib/meeting_Delete_Lib.xml'/>
    <puts text='---Quote_2528.xml---Finished!'/>   
</soda>
