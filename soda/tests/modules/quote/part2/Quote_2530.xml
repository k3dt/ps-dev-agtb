<soda>
    	<csv file='{@global.scriptsdir}/csvs/quote/Quote_2530.csv' var='Quote_2530'>
        <puts text='----Quote_2530.xml----Edit Task for Quote_Verify that task is related to this Quote can be removed successful.----Start!----'/>            
        <link id='moduleTab_Quotes'/>
        <browser assert='Quotes'/>
        <form id='search_form'>
            <button id='search_form_clear'/>
            <textfield id='name_basic' set='{@pre_Quote_name}'/>
            <button id='search_form_submit'/>
        </form>

        <table class='list view'>
            <link text='{@pre_Quote_name}'/>
        </table>

        <div id='list_subpanel_activities'>
            <table class='list view'>
                <button value='  Create Task  '/>
            </table>
        </div>
        
        <form id='form_SubpanelQuickCreate_Tasks'>
            <button id='Tasks_subpanel_full_form_button'/>
        </form>
        <timestamp/>

        <!--teams select-->
        <table id='EditView_team_name_table'>
            <button value='Select'/>
        </table>
        <browser assert='Team Search'/>
        <textfield name='name' set='{@Quote_2530.team_name}'/> 
        <button value='Search'/>
        <link text='{@Quote_2530.team_name}'/>   
        
        <form id='EditView'>
            <textfield id='name' set='{@Quote_2530.task_name} {@user.name} {@stamp}'/>
            <select id='priority' set='{@Quote_2530.task_priority}'/>
            <select id='status' set='{@Quote_2530.task_status}'/>  
            <button value='Save'/>
        </form>
         
        <div id='list_subpanel_activities'>
            <table class='list view'>
                <browser assert='{@Quote_2530.task_name} {@user.name} {@stamp}'/>
                <link text='{@Quote_2530.task_name} {@user.name} {@stamp}' click='false'/>
                <link class='listViewTdToolsS1' index='2' alert='true'/>             
                <wait timeout='2'/>
                <link text='{@Quote_2530.task_name} {@user.name} {@stamp}' click='false' exist='false'/>
            </table>
        </div>       
    </csv>
    <!--Run Delete XML-->
    <script file='{@global.scriptsdir}/modules/lib/Task_Delete_Lib.xml'/>
    <puts text='---Quote_2530.xml---Finished!'/>   
</soda>
