<soda>
    <csv file='{@global.scriptsdir}/csvs/quote/Quote_2537.csv' var='Quote_2537'>
        <puts text='----Quote_2537.xml----Quote Detail-History sub-panel_Verify that Quote(s) datail realated to this Quote can be viewed successfully.----Start!----'/>            
        <link id='moduleTab_Quotes'/>
        <browser assert='Quotes'/>
        <form id='search_form'>
            <button id='search_form_clear'/>
            <textfield id='name_basic' set='{@pre_Quote_name}'/>
            <button id='search_form_submit'/>
        </form>

        <table class='list view'>
            <link text='{@pre_Quote_name}'/>
        </table>
        
         
         <form id='formformNotes'>
            <button value='  Create Note or Attachment  '/>
         </form>         
         <timestamp/>
        <!--teams select-->
        <table id='form_SubpanelQuickCreate_Notes_team_name_table'>
            <button value='Select'/>
        </table>
        <browser assert='Team Search'/>
        <textfield name='name' set='{@Quote_2537.team_name}'/> 
        <button value='Search'/>
        <link text='{@Quote_2537.team_name}'/>
            
        <form id='form_SubpanelQuickCreate_Notes'>
            <textfield id='name' set='{@Quote_2537.note_name} {@user.name} {@stamp}'/>
            <textarea id='description' set='{@Quote_2537.note_description}'/>           
            <button id='Notes_subpanel_save_button'/>
        </form>
         
        <div id='list_subpanel_history'>
            <table class='list view'>             
                <link text='{@Quote_2537.note_name} {@user.name} {@stamp}' click='true'/>
            </table>
        </div>       
    </csv>
    <!--Run Delete XML-->
    <script file='{@global.scriptsdir}/modules/lib/Note_Delete_Lib.xml'/>
    <puts text='---Quote_2537.xml---Finished!'/>   
</soda>
