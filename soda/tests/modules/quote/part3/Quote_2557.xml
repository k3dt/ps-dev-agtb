<soda>
    	<csv file='{@global.scriptsdir}/csvs/quote/Quote_2557.csv' var='Quote_2557'>
        <puts text='----Quote_2557.xml----Relationship-Create Note or Attachment_Verify that new Note or Attachment can be canceled for Activites under "Quote" sub-panel.----Start!----'/>            
        <link id='moduleTab_Quotes'/>
        <browser assert='Quotes'/>
        <form id='search_form'>
            <button id='search_form_clear'/>
            <textfield id='name_basic' set='{@pre_Quote_name}'/>
            <button id='search_form_submit'/>
        </form>

        <table class='list view'>
            <link text='{@pre_Quote_name}'/>
        </table>
        
         
         <form id='formformNotes'>
            <button value='  Create Note or Attachment  '/>
         </form>         
         <timestamp/>
         
        <!--teams select-->
        <table id='form_SubpanelQuickCreate_Notes_team_name_table'>
            <button value='Select'/>
        </table>
        <browser assert='Team Search'/>
        <textfield name='name' set='{@Quote_2557.team_name}'/> 
        <button value='Search'/>
        <link text='{@Quote_2557.team_name}'/>
            
        <form id='form_SubpanelQuickCreate_Notes'>
            <textfield id='name' set='{@Quote_2557.note_name} {@user.name} {@stamp}'/>
            <textarea id='description' set='{@Quote_2557.note_description}'/>           
            <button id="Notes_subpanel_cancel_button" alert="true" />  <!-- bug 39160 -->/>
        </form>
         
      <div id='list_subpanel_history'>
           <table class='list view'>             
             <link text='{@Quote_2557.note_name} {@user.name} {@stamp}' click='false' exist='false'/>
           </table>
       </div>                
    </csv>
    <puts text='---Quote_2557.xml---Finished!'/>   
</soda>
