<soda>
	<!-- Edit Meeting for Quote_Verify that meeting related to this Quote can
		be removed successful.-->
    <puts text='Test: Quote_2531.xml started.' />
    <csv file='{@global.scriptsdir}/csvs/quote/Quote_2531.csv' var='Quote_2531'>
	<link id='moduleTab_Quotes'/>
        <browser assert='Quotes'/>
        <form id='search_form'>
            <button id='search_form_clear'/>
            <textfield id='name_basic' set='{@pre_Quote_name}'/>
            <button id='search_form_submit'/>
        </form>

        <table class='list view'>
            <link text='{@pre_Quote_name}'/>
        </table>
        
        <div id='list_subpanel_activities'>
            <table class='list view'>
                <button value='  Schedule Meeting  '/>
            </table>
        </div>
        
        <form id='form_SubpanelQuickCreate_Meetings'>
            <button id='Meetings_subpanel_full_form_button'/> 
        </form>
        
        <!--teams select-->
        <table id='EditView_team_name_table'>
                <button value='Select' />
        </table>
        <browser assert='Team Search' />
        <textfield name='name' set='{@Quote_2531.team_name}' />
        <button value='Search' />
        <link text='{@Quote_2531.team_name}' />
        <form id='EditView'>
                <textfield id='name'
                        set='{@Quote_2531.meeting_name} {@user.name} {@stamp}' />
                <textfield id='location' set='{@Quote_2531.meeting_location}' />
                <textfield id='description'
                        set='{@Quote_2531.meeting_description}' />
                <select id='status' set='{@Quote_2531.meeting_status}' />
                <select id='date_start_hours'
                        set='{@Quote_2531.meeting_date_start_hours}' />
                <select id='date_start_minutes'
                        set='{@Quote_2531.meeting_date_start_minutes}' />
                <!--select id='date_start_meridiem'
                        set='{@Quote_2531.meeting_date_start_meridiem}'/-->

                <textfield name='duration_hours'
                        set='{@Quote_2531.meeting_duration_hours}' />
                <select name='duration_minutes'
                        set='{@Quote_2531.meeting_duration_minutes}' />
                <checkbox name='reminder_checked'
                        set='{@Quote_2531.meeting_reminder_checked}' />
                <button value='Save' />
        </form>

        <div id='list_subpanel_activities'>
                <table class='list view'>
                        <browser assert='{@Quote_2531.meeting_name} {@user.name} {@stamp}' />
                        <link text='{@Quote_2531.meeting_name} {@user.name} {@stamp}'
                                click='false' />
                        <link class='listViewTdToolsS1' index='2' alert='true' />
                        <wait timeout='2'/>
                        <link text='{@Quote_2531.meeting_name} {@user.name} {@stamp}'
                                click='false' exist='false' />
                </table>
        </div>
    </csv>

    <!--Run Delete XML-->
    <script file='{@global.scriptsdir}/modules/lib/meeting_Delete_Lib.xml'/>
    <puts text='Test: Quote_2531.xml finished.' />
</soda>
