<soda>
    <csv file='{@global.scriptsdir}/csvs/quote/Quote_2541.csv' var='Quote_2541'>
        <puts text='----Quote_2541.xml----Quote Detail - History sub-panel_Verify that onte related to this Quote can be modified.----Start!----'/>            
        <link id='moduleTab_Quotes'/>
        <browser assert='Quotes'/>
        <form id='search_form'>
            <button id='search_form_clear'/>
            <textfield id='name_basic' set='{@pre_Quote_name}'/>
            <button id='search_form_submit'/>
        </form>

        <table class='list view'>
            <link text='{@pre_Quote_name}'/>
        </table>
        
         
         <form id='formformNotes'>
            <button value='  Create Note or Attachment  '/>
         </form>         
         <timestamp/>
        <!--teams select-->
        <table id='form_SubpanelQuickCreate_Notes_team_name_table'>
            <button value='Select'/>
        </table>
        <browser assert='Team Search'/>
        <textfield name='name' set='{@Quote_2541.team_name}'/> 
        <button value='Search'/>
        <link text='{@Quote_2541.team_name}'/>
            
        <form id='form_SubpanelQuickCreate_Notes'>
            <textfield id='name' set='{@Quote_2541.note_name} {@user.name} {@stamp}'/>
            <textarea id='description' set='{@Quote_2541.note_description}'/>           
            <button id='Notes_subpanel_save_button'/>
        </form>
        <wait timeout='5'/>
        <div id='list_subpanel_history'>
            <table class='list view'>             
                <browser assert='{@Quote_2541.note_name} {@user.name} {@stamp}'/>
                <link class='listViewTdToolsS1' index='1' />
            </table>
        </div>
        <wait timeout='2'/>
        <form id='EditView'>
            <textfield id='name' set='{@Quote_2541.note_name_edit}'/>
            <textarea id='description' set='{@Quote_2541.note_description_edit}'/>           
            <button value='Save'/>
        </form>
        <wait timeout='2'/>
        <div id='list_subpanel_history'>
            <table class='list view'>             
                <browser assert='{@Quote_2541.note_name_edit}'/>             
            </table>
        </div>
    </csv>
    <!--Run Delete XML-->
    <script file='{@global.scriptsdir}/modules/lib/Note_Delete_Lib.xml'/>
    <puts text='---Quote_2541.xml---Finished!'/>   
</soda>
