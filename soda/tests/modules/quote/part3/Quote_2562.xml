<soda>
    	<csv file='{@global.scriptsdir}/csvs/quote/Quote_2562.csv' var='Quote_2562'>
        <puts text="----Quote_2562.xml----Relationship-Create Contracts_Verify that new contracts can be created on the 'Contracts' sub-panel.----Start!----"/>            
        <link id='moduleTab_Quotes'/>
        <browser assert='Quotes'/>
        <form id='search_form'>
            <button id='search_form_clear'/>
            <textfield id='name_basic' set='{@pre_Quote_name}'/>
            <button id='search_form_submit'/>
        </form>

        <table class='list view'>
            <link text='{@pre_Quote_name}'/>
        </table>
         
        <div id='list_subpanel_contracts'>
            <table class='list view'>
                <button name='contracts_quotes_create_button'/>
            </table>
        </div>        
               
        <!--Teams select-->
        <table id='form_SubpanelEditView_Contracts_team_name_table'>
            <button value='Select'/>
        </table>
        <browser assert='Team Search'/>
        <textfield name='name' set='{@Quote_2562.team_name}'/> 
        <button value='Search'/>
        <link text='{@Quote_2562.team_name}'/>
                
        <!--Opportunity select-->
        <button id='btn_opportunity_name'/>
        <browser assert='Opportunity Search'/>
        <textfield name='name_advanced' set='{@pre_opp_name}'/> 
        <button value='Search'/>
        <table class='list view'>
            <link text='{@pre_opp_name}'/>
        </table>
                
        <!--Account select-->
        <button id='btn_account_name'/>
        <browser assert='Account Search'/>
        <textfield name='name_advanced' set='{@pre_account_name}'/> 
        <button value='Search'/>
        <table class='list view'>
            <link text='{@pre_account_name}'/>
        </table>
        <wait timeout='3'/> 
        <form id='form_SubpanelEditView_Contracts'>
            <textfield name='name' set='{@Quote_2562.contract_name_edit}'/>
            <select id='status' set='{@Quote_2562.contract_status_edit}'/>
            <button id='Contracts_subpanel_save_button'/>
        </form>
    </csv>
    <!--Run Delete XML-->
    <link href='index.php?module=Contracts&amp;action=index' />
    <button id='search_form_clear' />
    <textfield name='name_basic' set='{@Quote_2562.contract_name_edit}'/>
    <button id='search_form_submit'/>
    <checkbox name='massall' click='true' />
    <link id='actions_link' />
    <link text='Delete' alert='true' />
    <puts text='---Quote_2562.xml---Finished!'/>   
</soda>
