<soda>
	<!-- Schedule Meeting_Verify that scheduled meeting cannot be created for case when entering non-existing data (such as entering non-existing data in "Team" field) for the "Schedule Meeting" function -->
	<puts text="Test: Cases_1356.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/cases/CasesAssertText.csv" var="cases" />
	<timestamp />

	<!--Create a case record-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Create_Lib.xml" />
	<var var="case_record" set="{@pre_case_name}" />

	<!--go to the case-->
	<link id="moduleTab_Cases" />
	<div class="moduleTitle">
		<link href="index.php?module=Cases&amp;action=index" click="false" />
	</div>

	<button value="Clear" />
	<textfield id="name_basic" set="{@case_record}" />
	<button id="search_form_submit" />
	<link text="{@case_record}" />

	<!--create a Meeting-->
	<div id="list_subpanel_activities">
		<button value="  Schedule Meeting  " />
	</div>

	<csv file="{@global.scriptsdir}/csvs/cases/Case_1356.csv" var="Case_1356">
		<textfield id="name" set="{@Case_1356.name} {@user.name} {@stamp}" />
		<textfield id="team_name_collection_0" set=" " />
		<button id="Meetings_subpanel_save_button" />
		<browser assert="No Primary Team specified" />
	</csv>
	
	<link id="moduleTab_Cases" alert="true" />

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Delete_Lib.xml" />

	<puts text="Test: Cases_1356.xml finished." />
</soda>