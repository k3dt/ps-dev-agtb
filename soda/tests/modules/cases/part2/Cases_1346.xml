<soda>
	<!-- Remove Contact_Verify that contact can be removed under case when using "rem" function -->
	<puts text="Test: Cases_1346.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/cases/CasesAssertText.csv" var="cases" />

	<!--Create a case record-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Create_Lib.xml" />
	<var var="case_record" set="{@pre_case_name}" />

	<!--add contact to case-->
	<link id="moduleTab_Cases" />
	<div class="moduleTitle">
		<link href="index.php?module=Cases&amp;action=index" click="false" />
	</div>

	<button value="Clear" />
	<button id="search_form_submit" />

	<link text="{@case_record}" />

	<csv file="{@global.scriptsdir}/csvs/cases/Case_1346.csv" var="Case_1346">
		<div id="list_subpanel_contacts">
			<button id="contacts_cases_create_button" />
			<wait timeout="3" />
		</div>
		
		<form id="form_SubpanelQuickCreate_Contacts">
			<textfield id="last_name" set="{@Case_1346.last_name}" />
			<button id="Contacts_subpanel_save_button" />
			<wait timeout="3" />
		</form>

		<div id="list_subpanel_contacts">
			<table class="list view">
				<span class="pageNumbers" assert="(1 - 1 of 1)" />
			</table>
		</div>

		<!--delete the contact-->
		<div id="list_subpanel_contacts">
			<link class="listViewTdToolsS1" index="2" alert="true" />
		</div>
		<wait timeout="2" />
		<div id="list_subpanel_contacts">
			<table class="list view">
				<span class="pageNumbers" assert="(0 - 0 of 0)" />
			</table>
		</div>
	</csv>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Contact_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Case_Delete_Lib.xml" />

	<puts text="Test: Cases_1346.xml finished." />
</soda>
