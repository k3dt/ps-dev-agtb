<soda>
	<!-- Duplicate_case -->
	<puts text="Test: Cases_8225.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/cases/CasesAssertText.csv" var="cases" />

	<!--Create a case record-->
	<!-- Below is TEMPORARY -->
	<script file="{@global.scriptsdir}/modules/lib/Case_Create_Lib_8225.xml" />
	<!--
	This is the old value:
	<script file="{@global.scriptsdir}/modules/lib/Case_Create_Lib.xml" />
	-->
	<var var="case_record" set="{@pre_case_name}" />

	<!--go to the case-->
	<link id="moduleTab_Cases" />
	<div class="moduleTitle">
		<link href="index.php?module=Cases&amp;action=index" click="false" />
	</div>

	<button value="Clear" />
	<textfield id="name_basic" set="{@case_record}" />
	<button id="search_form_submit" />
	<link text="{@case_record}" />

	<!--duplicate case-->
	<button id="duplicate_button" />

	<csv file="{@global.scriptsdir}/csvs/cases/Case_8225.csv" var="Case_8225">
		<textfield id="name" set="{@Case_8225.name}" />
		<button value="Save" />
	</csv>

	<link id="moduleTab_Cases" />
	<div class="moduleTitle">
		<link href="index.php?module=Cases&amp;action=index" click="false" />
	</div>

	<button value="Clear" />
	<textfield id="name_basic" set="{@case_record}" />
	<button id="search_form_submit" />
	<span class="pageNumbers" assert="(1 - 1 of 1)" />

	<button value="Clear" />
	<textfield id="name_basic" set="{@Case_8225.name}" />
	<button id="search_form_submit" />
	<span class="pageNumbers" assert="(1 - 1 of 1)" />

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Delete_Lib.xml" />

	<puts text="Test: Cases_8225.xml finished." />
</soda>