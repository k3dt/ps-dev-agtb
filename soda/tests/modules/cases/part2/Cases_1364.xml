<soda>
   <!-- Remove Scheduled Meeting_Verify that meeting can be removed under 
      case when using "rem" function 
   -->
	<puts text="Test: Cases_1364.xml starting." />
   <csv file="{@global.scriptsdir}/csvs/cases/CasesAssertText.csv" 
      var="cases" />

	<!--Create a case record-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Create_Lib.xml" />
	<var var="case_record" set="{@pre_case_name}" />

	<!--go to the case-->
	<link id="moduleTab_Cases" />
	<div class="moduleTitle">
		<link href="index.php?module=Cases&amp;action=index" click="false" />
	</div>

	<button value="Clear" />
	<textfield id="name_basic" set="{@case_record}" />
	<button id="search_form_submit" />
	<link text="{@case_record}" />

	<!--create a Meeting-->
	<div id="list_subpanel_activities">
      <button value="  Schedule Meeting  " />
      <wait timeout="3" />
	</div>
	<timestamp />

	<csv file="{@global.scriptsdir}/csvs/cases/Case_1364.csv" var="Case_1364">
      <div id="DEFAULT">
		   <textfield id="name" set="{@Case_1364.name} {@user.name} {@stamp}" />
      </div>
		<button id="Meetings_subpanel_save_button" />
	</csv>
	<div id="list_subpanel_activities">
		<table class="list view">
			<link text="{@Case_1364.name} {@user.name} {@stamp}" click="false" />
		</table>

		<link class="listViewTdToolsS1" index="2" alert="true" />
	</div>
	<wait timeout="5" />
	<div id="list_subpanel_activities">
		<span class="pageNumbers" assert="(0 - 0 of 0)" />
	</div>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Meeting_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Case_Delete_Lib.xml" />
	<puts text="Test: Cases_1364.xml finished." />
</soda>
