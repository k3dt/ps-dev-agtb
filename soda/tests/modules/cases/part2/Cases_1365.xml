<soda>
	<!-- Schedule Call_Verify that scheduled call cannot be created for case when non-existing data (such as entering non-existing data in "Team" field) are entered for the "Schedule Call" function -->
	<puts text="Test: Cases_1365.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/cases/CasesAssertText.csv" var="cases" />
	<timestamp />

	<!--Create a case record-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Create_Lib.xml" />
	<var var="case_record" set="{@pre_case_name}" />

	<!--go to the case-->
	<link id="moduleTab_Cases" />
	<div class="moduleTitle">
		<link href="index.php?module=Cases&amp;action=index" click="false" />
	</div>

	<button value="Clear" />
	<textfield id="name_basic" set="{@case_record}" />
	<button id="search_form_submit" />
	<link text="{@case_record}" />

	<!--create Call-->
	<div id="list_subpanel_activities">
		<button value="  Log Call  " />
	</div>

	<csv file="{@global.scriptsdir}/csvs/cases/Case_1365.csv" var="Case_1365">
		<form id="form_SubpanelQuickCreate_Calls">
			<textfield id="name" set="{@Case_1365.name} {@user.name} {@stamp}" />
			<!--<image id="remove_team_name_collection_0" click="true" />-->
			<textfield id="team_name_collection_0" set="{@Case_1365.team}" />
			<button id="Calls_subpanel_save_button" />
		</form>
	</csv>
	<browser assert="No Primary Team specified" />
	
	<link id="moduleTab_Cases" alert="true" />

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Delete_Lib.xml" />

	<puts text="Test: Cases_1365.xml finished." />
</soda>