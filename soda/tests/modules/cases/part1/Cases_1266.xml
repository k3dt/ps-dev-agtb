<soda>
	<!-- Delete Case_Verify that case can be deleted when using "Delete" function in "Case" detail view -->
	<puts text="Test: Cases_1266.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/cases/CasesAssertText.csv" var="cases" />

	<!--Create case-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Create_Lib.xml" />

	<link id="moduleTab_Cases" />
	<div class="moduleTitle">
		<link href="index.php?module=Cases&amp;action=index" click="false" />
	</div>

	<!--search case-->
	<button id="search_form_submit" />
	<div id="Casesbasic_searchSearchForm">
		<textfield id="name_basic" set="{@case_record.name} {@user.name} {@stamp}" />
	</div>
	<button id="search_form_submit" />
	<link text="{@case_record.name} {@user.name} {@stamp}" />

	<!--delete case-->
	<form name="DetailView">
		<button name="Delete" alert="true" />
	</form>

	<!--verify-->
	<link id="moduleTab_Cases" />
	<div class="moduleTitle">
		<link href="index.php?module=Cases&amp;action=index" click="false" />
	</div>

	<button name="clear" />
	<checkbox name="massall" set="true" />
	<link text="Actions" />
	<link text="Delete" alert="true" />
	<table class="list view">
		<span class="pageNumbers" assert="(0 - 0 of 0)" />
	</table> 

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Delete_Lib.xml" />

	<puts text="Test: Cases_1266.xml finished." />
</soda>