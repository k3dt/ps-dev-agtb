<soda>
	<!-- Create Case_Verify that case can be created by edit view -->
	<puts text="Test: Cases_1262.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/cases/CasesAssertText.csv" var="cases" />

	<!--Create case-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Create_Lib_Full.xml" />

	<!--Assert Data-->
	<browser assert="{@case_record.name} {@user.name} {@stamp}" />
	<browser assert="{@case_record.description}" />
	<browser assert="{@case_record.status}" />
	<browser assert="{@case_record.priority}" />
	<browser assert="{@case_record.type}" />
	<browser assert="{@case_record.resolution}" />

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Delete_Lib.xml" />

	<puts text="Test: Cases_1262.xml finished." />
</soda>