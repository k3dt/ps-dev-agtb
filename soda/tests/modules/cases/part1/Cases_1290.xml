<soda>
	<!-- Edit Scheduled Meeting_Verify that the information of a scheduled meeting for case cannot be modified when non-existing data (such as entering non-existing data in "Team" field) are entered when using "Edit" function in "Activities" sub-panel -->
	<puts text="Test: Cases_1290.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/cases/CasesAssertText.csv" var="cases" />

	<!--Create case-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Create_Lib.xml" />

	<link id="moduleTab_Cases" />
	<div class="moduleTitle">
		<link href="index.php?module=Cases&amp;action=index" click="false" />
	</div>

	<!--search case-->
	<div id="Casesbasic_searchSearchForm">
		<textfield id="name_basic" set="{@pre_case_name}" />
	</div>
	<button id="search_form_submit" />
	<link text="{@pre_case_name}" />

	<!--create a meeting-->
	<csv file="{@global.scriptsdir}/csvs/cases/Case_1290.csv" var="Case_1290">
		<timestamp />
		<div id="list_subpanel_activities">
			<button value="  Schedule Meeting  " />
		</div>

		<form id="form_SubpanelQuickCreate_Meetings">
			<button id="Meetings_subpanel_full_form_button" />
		</form>

		<!---meeting code-->
		<textfield id="name" set="{@Case_1290.name} {@user.name} {@stamp}" />
		<textfield id="location" set="{@Case_1290.location}" />
		<textfield id="description" set="{@Case_1290.description}" />
		<select id="status" set="{@Case_1290.status}" />

		<textfield id="date_start_date" set="{@Case_1290.date_start_date}" />
		<select id="date_start_hours" set="{@Case_1290.date_start_hours}" />
		<select id="date_start_minutes" set="{@Case_1290.date_start_minutes}" />

		<textfield name="duration_hours" set="{@Case_1290.duration_hours}" />
		<select name="duration_minutes" set="{@Case_1290.duration_minutes}" />
		<checkbox name="reminder_checked" set="{@Case_1290.reminder_checked}" />

		<form id="EditView">
			<button value="Save" />
		</form>
		<div id="list_subpanel_activities">
			<table class="list view">
				<link text="{@Case_1290.name} {@user.name} {@stamp}" click="false" />
			</table>

			<link class="listViewTdToolsS1" index="1" />
		</div>
		<image id="remove_team_name_collection_0" click="true" />
		<form id="EditView">
			<button value="Save" />
		</form>
		<browser assert="No Primary Team specified" />

		<button value="Cancel" alert="true" />
	</csv>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Delete_Lib.xml" />
	<script file="{@global.scriptsdir}/modules/lib/Meeting_Delete_Lib.xml" />

	<puts text="Test: Cases_1290.xml finished." />
</soda>