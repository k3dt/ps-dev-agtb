<soda>
	<!-- Edit Case_Verify that case is not changed when using "Cancel" function in "Case" edit view -->
	<puts text="Test: Cases_1333.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/cases/CasesAssertText.csv" var="cases" />
	<timestamp />

	<!--Create a case record-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Create_Lib.xml" />

	<!--go to case module and edit-->
	<link id="moduleTab_Cases" />
	<div class="moduleTitle">
		<link href="index.php?module=Cases&amp;action=index" click="false" />
	</div>

	<textfield id="name_basic" set="{@pre_case_name}" />
	<button id="search_form_submit" />

	<link text="{@pre_case_name}" />

	<button id="edit_button" />

	<!--create case record from quick create-->
	<button id="btn_clr_account_name" />
	<csv file="{@global.scriptsdir}/csvs/cases/Case_1333.csv" var="case_1333">
		<textfield id="name" set="{@case_1333.name}" />
	</csv>
	<button value="Cancel" alert="true" />

	<link id="moduleTab_Cases" />
	<div class="moduleTitle">
		<link href="index.php?module=Cases&amp;action=index" click="false" />
	</div>

	<textfield id="name_basic" set="{@pre_case_name}" />
	<button id="search_form_submit" />
	<table class="list view">
		<span class="pageNumbers" assert="(1 - 1 of 1)" />
	</table>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Delete_Lib.xml" />

	<puts text="Test: Cases_1333.xml finished." />
</soda>