<soda>
	<!-- Create Case_Verify that case can be duplicated when using "Duplicate" function in "Case" detail view -->
	<puts text="Test: Cases_1263.xml starting." />
	<csv file="{@global.scriptsdir}/csvs/cases/CasesAssertText.csv" var="cases" />

	<!--Create case-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Create_Lib.xml" />

	<csv file="{@global.scriptsdir}/csvs/cases/Case_1263.csv" var="dup_case">
		<!--dup case-->
		<form name="DetailView">
			<button id="duplicate_button" />
		</form>

		<timestamp />
		<textfield id="name" set="{@dup_case.New_name} {@user.name} {@stamp}" />

		<button value="Save" />
		<browser assert="{@dup_case.New_name} {@user.name} {@stamp}" />

		<link id="moduleTab_Cases" />
		<div class="moduleTitle">
			<link href="index.php?module=Cases&amp;action=index" click="false" />
		</div>

		<button value="Clear" />

		<!--verify-->
		<table class="list view">
			<span class="pageNumbers" assert="(1 - 2 of 2)" />
		</table>
	</csv>

	<!--Delete the record-->
	<script file="{@global.scriptsdir}/modules/lib/Case_Delete_Lib.xml" />

	<puts text="Test: Cases_1263.xml finished." />
</soda>
