<h1 class="nomad">Choose link type</h1>
<div class="container-fluid">
    <div class="row-fluid">
        <div class="span12 listing" id="listing"><i></i>
            <div>
                {{#each items}}
                <article class="picker-list-row">
                    <a href="#{{linkRoute ../context this.name ../action}}?depth=2">
                        {{this.name}}
                    </a>
                </article>
                {{/each}}
            </div>
        </div>
    </div>
</div>
<fieldset>
    <div>
        <a href="javascript:void(0)" class="btn btn-inverse menu-cancel" id="backRecord">Back</a>
    </div>
</fieldset>