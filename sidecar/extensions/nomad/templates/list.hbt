<h1 class="nomad">{{context.state.module}}</h1>
<div class="container-fluid">
  <div class="row-fluid">
      <div class="span12 listing fav filtered" id="listing-{{context.state.module}}">
          <i></i>
          <section class="search topelbar">
            <i class="icon-search"></i>
            <div class="form-search row-fluid">
              <input type="text" class="search-query" placeholder="Search all {{context.state.module}}">
              <aside>
                <div class="btn-group">
                  <a title="Starred" href="#" class="btn"><i class="icon-star"></i></a>
                  <a title="Account" href="#" class="btn"><i class="icon-user"></i></a>
                </div>
              </aside>
            </div>
          </section>
          {{#each meta.panels}}
          <div class="block-footer show-more-top-btn" style="display: none">
              <a class="btn loading wide " href="javascript:void(0)">Show More</a>
          </div>
          <div class="items">
          {{#each ../context.state.collection.models}}
              {{listItem this ../../this ../fields}}
          {{/each}}
          </div>
          <div class="block-footer show-more-bottom-btn">
              <a class="btn loading wide" href="javascript:void(0)">Show More</a>
          </div>
          {{/each}}
      </div>
    </div>
</div>
